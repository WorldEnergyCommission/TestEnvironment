	lr_Begleitheizung_Q : LREAL;			//{#lynus.ag#()}
	lr_Begleitheizung_PF : LREAL;			//{#lynus.ag#()}

	lr_Nebenantriebe_Freq : LREAL;			//{#lynus.ag#()}
	lr_Nebenantriebe_L1N : LREAL;			//{#lynus.ag#()}
	lr_Nebenantriebe_L2N : LREAL;			//{#lynus.ag#()}
	lr_Nebenantriebe_L3N : LREAL;			//{#lynus.ag#()}
	lr_Nebenantriebe_L1L2 : LREAL;			//{#lynus.ag#()}
	lr_Nebenantriebe_L2L3 : LREAL;			//{#lynus.ag#()}
	lr_Nebenantriebe_L3L1 : LREAL;			//{#lynus.ag#()}
	lr_Nebenantriebe_I1 : LREAL;			//{#lynus.ag#()}
	lr_Nebenantriebe_I2 : LREAL;			//{#lynus.ag#()}
	lr_Nebenantriebe_I3 : LREAL;			//{#lynus.ag#()}
	lr_Nebenantriebe_N : LREAL;				//{#lynus.ag#()}
	lr_Nebenantriebe_S : LREAL;				//{#lynus.ag#()}
	lr_Nebenantriebe_P : LREAL;				//{#lynus.ag#()}
	lr_Nebenantriebe_Q : LREAL;				//{#lynus.ag#()}
	lr_Nebenantriebe_PF : LREAL;			//{#lynus.ag#()}
	
	lr_Rest_P : LREAL;								//{#lynus.ag#()}
	lr_GasmessungWTMenge_m3h 	: LREAL;			//{#lynus.ag#()}
	lr_GasmessungWTMenge_m3h_2 	: LREAL;			//{#lynus.ag#()}
	lr_GasmessungWTTemp_C 		: LREAL;			//{#lynus.ag#()}
	lr_GasmessungWTDruck_bar 	: LREAL;			//{#lynus.ag#()}
	m_Brennerstart			 	: BOOL;			//{#lynus.ag#()}
	m_Mischerstart			 	: BOOL;			//{#lynus.ag#()}
