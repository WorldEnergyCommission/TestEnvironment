	lr_VerteilerRC_Kalt_Freq : LREAL;		//{#lynus.ag#()}
	lr_VerteilerRC_Kalt_L1N : LREAL;		//{#lynus.ag#()}
	lr_VerteilerRC_Kalt_L2N : LREAL;		//{#lynus.ag#()}
	lr_VerteilerRC_Kalt_L3N : LREAL;		//{#lynus.ag#()}
	lr_VerteilerRC_Kalt_L1L2 : LREAL;		//{#lynus.ag#()}
	lr_VerteilerRC_Kalt_L2L3 : LREAL;		//{#lynus.ag#()}
	lr_VerteilerRC_Kalt_L3L1 : LREAL;		//{#lynus.ag#()}
	lr_VerteilerRC_Kalt_I1 : LREAL;			//{#lynus.ag#()}
	lr_VerteilerRC_Kalt_I2 : LREAL;			//{#lynus.ag#()}
	lr_VerteilerRC_Kalt_I3 : LREAL;			//{#lynus.ag#()}
	lr_VerteilerRC_Kalt_N : LREAL;			//{#lynus.ag#()}
	lr_VerteilerRC_Kalt_S : LREAL;			//{#lynus.ag#()}
	lr_VerteilerRC_Kalt_P : LREAL;			//{#lynus.ag#()}
	lr_VerteilerRC_Kalt_Q : LREAL;			//{#lynus.ag#()}
	lr_VerteilerRC_Kalt_PF : LREAL;			//{#lynus.ag#()}
	

	lr_Begleitheizung_Freq : LREAL;			//{#lynus.ag#()}
	lr_Begleitheizung_L1N : LREAL;			//{#lynus.ag#()}
	lr_Begleitheizung_L2N : LREAL;			//{#lynus.ag#()}
	lr_Begleitheizung_L3N : LREAL;			//{#lynus.ag#()}
