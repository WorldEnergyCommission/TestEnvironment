    <Implementation>
      <ST><![CDATA[
lr_Rest_P := LIMIT(	0,
					lr_Mischanlage_Gesamt_P - lr_VerteilerRC_Kalt_P - lr_Begleitheizung_P - lr_Nebenantriebe_P
						- prgE5.lr_RC_Anlage_P - prgE5.lr_RC_Trommel_P  
						- prgE9.lr_Bitumentank_4_P - prgE9.lr_Dosierpumpe_1_P - prgE9.lr_Bitumenfuellpumpe_1_P - prgE9.lr_Dosierpumpe_2_P  - prgE9.lr_Bitumentank_1_P - prgE9.lr_Bitumentank_2_P - prgE9.lr_Bitumentank_3_P
					,1000);
lr_Rest_P_Temp :=	lr_Mischanlage_Gesamt_P - lr_VerteilerRC_Kalt_P - lr_Begleitheizung_P - lr_Nebenantriebe_P
						- prgE5.lr_RC_Anlage_P - prgE5.lr_RC_Trommel_P  
						- prgE9.lr_Bitumentank_4_P - prgE9.lr_Dosierpumpe_1_P - prgE9.lr_Bitumenfuellpumpe_1_P - prgE9.lr_Dosierpumpe_2_P  - prgE9.lr_Bitumentank_1_P - prgE9.lr_Bitumentank_2_P - prgE9.lr_Bitumentank_3_P;
CASE i_State_ModbusCall OF
	0: 	// wait state
		i_State_ModbusCall := 10;
	
	10:	// first Modbus Poll Call
		FB_Read_Socomec_I61.m_Start := TRUE;
		FB_Read_Socomec_I61.byte_slaveID := 3;
		FB_Read_Socomec_I61.w_Adress := 18442;
		i_State_ModbusCall := i_State_ModbusCall + 1;
	
	11: // wait for received data
		FB_Read_Socomec_I61.m_Start := FALSE;
		IF FB_Read_Socomec_I61.iStateModbusRead = 30 THEN
			lr_Mischanlage_Gesamt_Freq	:=		FB_Read_Socomec_I61.lr_Freq_Hz	;	
			lr_Mischanlage_Gesamt_L1N	:=		FB_Read_Socomec_I61.lr_L1N_V	;	
			lr_Mischanlage_Gesamt_L2N	:=		FB_Read_Socomec_I61.lr_L2N_V	;	
			lr_Mischanlage_Gesamt_L3N	:=		FB_Read_Socomec_I61.lr_L3N_V	;	
			lr_Mischanlage_Gesamt_L1L2	:=		FB_Read_Socomec_I61.lr_L1L2_V	;	
			lr_Mischanlage_Gesamt_L2L3	:=		FB_Read_Socomec_I61.lr_L2L3_V	;	
			lr_Mischanlage_Gesamt_L3L1	:=		FB_Read_Socomec_I61.lr_L3L1_V	;	
			lr_Mischanlage_Gesamt_I1	:=		FB_Read_Socomec_I61.lr_I1_A		;	
			lr_Mischanlage_Gesamt_I2	:=		FB_Read_Socomec_I61.lr_I2_A		;	
			lr_Mischanlage_Gesamt_I3	:=		FB_Read_Socomec_I61.lr_I3_A		;	
			lr_Mischanlage_Gesamt_N		:=		FB_Read_Socomec_I61.lr_N_A		;	
			lr_Mischanlage_Gesamt_P		:=		FB_Read_Socomec_I61.lr_P_kW		;	
			lr_Mischanlage_Gesamt_Q		:=		FB_Read_Socomec_I61.lr_Q_kVAr	;	
			lr_Mischanlage_Gesamt_S		:=		FB_Read_Socomec_I61.lr_S_kVA	;	
			lr_Mischanlage_Gesamt_PF	:=		FB_Read_Socomec_I61.lr_PF		;		
			
			i_State_ModbusCall := i_State_ModbusCall +1;
		END_IF            
		IF FB_Read_Socomec_I61.iStateModbusError = 300 OR ton_TimeOutState.Q THEN
			i_State_ModbusCall := i_State_ModbusCall +1;
		END_IF
	
	12: // wait for FB_Read_Socomec_I61 to be in state 0
		IF FB_Read_Socomec_I61.iStateModbusRead = 0 OR ton_TimeOutState.Q THEN
			i_State_ModbusCall := i_State_ModbusCall + 8;
		END_IF

	20:	// next Modbus Poll Call
		FB_Read_Socomec_I61.m_Start := TRUE;
		FB_Read_Socomec_I61.byte_slaveID := 4;
		FB_Read_Socomec_I61.w_Adress := 18442;
		i_State_ModbusCall := i_State_ModbusCall + 1;
	
	21: // wait for received data
		FB_Read_Socomec_I61.m_Start := FALSE;
		IF FB_Read_Socomec_I61.iStateModbusRead = 30 THEN
			lr_VerteilerRC_Kalt_Freq	:=		FB_Read_Socomec_I61.lr_Freq_Hz	;	
			lr_VerteilerRC_Kalt_L1N		:=		FB_Read_Socomec_I61.lr_L1N_V	;	
			lr_VerteilerRC_Kalt_L2N		:=		FB_Read_Socomec_I61.lr_L2N_V	;	
			lr_VerteilerRC_Kalt_L3N		:=		FB_Read_Socomec_I61.lr_L3N_V	;	
			lr_VerteilerRC_Kalt_L1L2	:=		FB_Read_Socomec_I61.lr_L1L2_V	;	
			lr_VerteilerRC_Kalt_L2L3	:=		FB_Read_Socomec_I61.lr_L2L3_V	;	
			lr_VerteilerRC_Kalt_L3L1	:=		FB_Read_Socomec_I61.lr_L3L1_V	;	
			lr_VerteilerRC_Kalt_I1		:=		FB_Read_Socomec_I61.lr_I1_A		;	
			lr_VerteilerRC_Kalt_I2		:=		FB_Read_Socomec_I61.lr_I2_A		;	
			lr_VerteilerRC_Kalt_I3		:=		FB_Read_Socomec_I61.lr_I3_A		;	
			lr_VerteilerRC_Kalt_N		:=		FB_Read_Socomec_I61.lr_N_A		;	
			lr_VerteilerRC_Kalt_P		:=		FB_Read_Socomec_I61.lr_P_kW		;	
			lr_VerteilerRC_Kalt_Q		:=		FB_Read_Socomec_I61.lr_Q_kVAr	;	
			lr_VerteilerRC_Kalt_S		:=		FB_Read_Socomec_I61.lr_S_kVA	;	
			lr_VerteilerRC_Kalt_PF		:=		FB_Read_Socomec_I61.lr_PF		;		
			
			i_State_ModbusCall := i_State_ModbusCall +1;
		END_IF            
		IF FB_Read_Socomec_I61.iStateModbusError = 300 OR ton_TimeOutState.Q THEN
			i_State_ModbusCall := i_State_ModbusCall +1;
		END_IF
	
	22: // wait for FB_Read_Socomec_I61 to be in state 0
		IF FB_Read_Socomec_I61.iStateModbusRead = 0 OR ton_TimeOutState.Q THEN
			i_State_ModbusCall := i_State_ModbusCall + 8;
		END_IF
	
	30:	// next Modbus Poll Call
		FB_Read_Socomec_I61.m_Start := TRUE;
		FB_Read_Socomec_I61.byte_slaveID := 3;
		FB_Read_Socomec_I61.w_Adress := 20490;
		i_State_ModbusCall := i_State_ModbusCall + 1;
	
	31: // wait for received data
		FB_Read_Socomec_I61.m_Start := FALSE;
		IF FB_Read_Socomec_I61.iStateModbusRead = 30 THEN
			lr_Begleitheizung_Freq	:=		FB_Read_Socomec_I61.lr_Freq_Hz	;	
			lr_Begleitheizung_L1N	:=		FB_Read_Socomec_I61.lr_L1N_V	;	
			lr_Begleitheizung_L2N	:=		FB_Read_Socomec_I61.lr_L2N_V	;	
			lr_Begleitheizung_L3N	:=		FB_Read_Socomec_I61.lr_L3N_V	;	
			lr_Begleitheizung_L1L2	:=		FB_Read_Socomec_I61.lr_L1L2_V	;	
			lr_Begleitheizung_L2L3	:=		FB_Read_Socomec_I61.lr_L2L3_V	;	
			lr_Begleitheizung_L3L1	:=		FB_Read_Socomec_I61.lr_L3L1_V	;	
			lr_Begleitheizung_I1	:=		FB_Read_Socomec_I61.lr_I1_A		;	
			lr_Begleitheizung_I2	:=		FB_Read_Socomec_I61.lr_I2_A		;	
			lr_Begleitheizung_I3	:=		FB_Read_Socomec_I61.lr_I3_A		;	
			lr_Begleitheizung_N		:=		FB_Read_Socomec_I61.lr_N_A		;	
			lr_Begleitheizung_P		:=		FB_Read_Socomec_I61.lr_P_kW		;	
			lr_Begleitheizung_Q		:=		FB_Read_Socomec_I61.lr_Q_kVAr	;	
			lr_Begleitheizung_S		:=		FB_Read_Socomec_I61.lr_S_kVA	;	
			lr_Begleitheizung_PF	:=		FB_Read_Socomec_I61.lr_PF		;		
			
			i_State_ModbusCall := i_State_ModbusCall +1;
		END_IF            
		IF FB_Read_Socomec_I61.iStateModbusError = 300 OR ton_TimeOutState.Q THEN
			i_State_ModbusCall := i_State_ModbusCall +1;
		END_IF
	
	32: // wait for FB_Read_Socomec_I61 to be in state 0
		IF FB_Read_Socomec_I61.iStateModbusRead = 0 OR ton_TimeOutState.Q THEN
			i_State_ModbusCall := i_State_ModbusCall + 8;
		END_IF
	
	40:	// next Modbus Poll Call
		FB_Read_Socomec_I61.m_Start := TRUE;
		FB_Read_Socomec_I61.byte_slaveID := 4;
		FB_Read_Socomec_I61.w_Adress := 20490;
		i_State_ModbusCall := i_State_ModbusCall + 1;
	
	41: // wait for received data
		FB_Read_Socomec_I61.m_Start := FALSE;
		IF FB_Read_Socomec_I61.iStateModbusRead = 30 THEN
			lr_Nebenantriebe_Freq	:=		FB_Read_Socomec_I61.lr_Freq_Hz	;	
			lr_Nebenantriebe_L1N	:=		FB_Read_Socomec_I61.lr_L1N_V	;	
			lr_Nebenantriebe_L2N	:=		FB_Read_Socomec_I61.lr_L2N_V	;	
			lr_Nebenantriebe_L3N	:=		FB_Read_Socomec_I61.lr_L3N_V	;	
			lr_Nebenantriebe_L1L2	:=		FB_Read_Socomec_I61.lr_L1L2_V	;	
			lr_Nebenantriebe_L2L3	:=		FB_Read_Socomec_I61.lr_L2L3_V	;	
			lr_Nebenantriebe_L3L1	:=		FB_Read_Socomec_I61.lr_L3L1_V	;	
			lr_Nebenantriebe_I1		:=		FB_Read_Socomec_I61.lr_I1_A		;	
			lr_Nebenantriebe_I2		:=		FB_Read_Socomec_I61.lr_I2_A		;	
			lr_Nebenantriebe_I3		:=		FB_Read_Socomec_I61.lr_I3_A		;	
			lr_Nebenantriebe_N		:=		FB_Read_Socomec_I61.lr_N_A		;	
			lr_Nebenantriebe_P		:=		FB_Read_Socomec_I61.lr_P_kW		;	
			lr_Nebenantriebe_Q		:=		FB_Read_Socomec_I61.lr_Q_kVAr	;	
			lr_Nebenantriebe_S		:=		FB_Read_Socomec_I61.lr_S_kVA	;	
			lr_Nebenantriebe_PF		:=		FB_Read_Socomec_I61.lr_PF		;		
			
			i_State_ModbusCall := i_State_ModbusCall +1;
		END_IF            
		IF FB_Read_Socomec_I61.iStateModbusError = 300 OR ton_TimeOutState.Q THEN
			i_State_ModbusCall := i_State_ModbusCall +1;
		END_IF
	
	42: // wait for FB_Read_Socomec_I61 to be in state 0
		IF FB_Read_Socomec_I61.iStateModbusRead = 0 OR ton_TimeOutState.Q THEN
			i_State_ModbusCall := i_State_ModbusCall + 8;
		END_IF


	50:	// next Modbus Poll Call		first IO20 call
		FB_Read_Socomec_IO20.m_Start := TRUE;
		FB_Read_Socomec_IO20.byte_slaveID := 5;
		FB_Read_Socomec_IO20.w_Adress := 11589;
		i_State_ModbusCall := i_State_ModbusCall + 1;
	
	51: // wait for received data
		FB_Read_Socomec_IO20.m_Start := FALSE;
		IF FB_Read_Socomec_IO20.iStateModbusRead = 30 THEN
			lr_GasmessungWTMenge_m3h := LIMIT(0,2150.0/16.0*(FB_Read_Socomec_IO20.lr_Value_mA-4),2150);		// 4-20mA = 0-2150 m3/h
			lr_GasmessungWTMenge_m3h_2 := LIMIT(0,FB_Read_Socomec_IO20.lr_Value_mA,2150);					// plain Output Socomec
			i_State_ModbusCall := i_State_ModbusCall +1;
		END_IF            
		IF FB_Read_Socomec_I61.iStateModbusError = 300 OR ton_TimeOutState.Q THEN
			i_State_ModbusCall := i_State_ModbusCall +1;
		END_IF
	
	52: // wait for FB_Read_Socomec_IO20 to be in state 0
		IF FB_Read_Socomec_IO20.iStateModbusRead = 0 OR ton_TimeOutState.Q THEN
			i_State_ModbusCall := i_State_ModbusCall + 8;
		END_IF
		
	60:	// next Modbus Poll Call		second IO20 call
		FB_Read_Socomec_IO20.m_Start := TRUE;
		FB_Read_Socomec_IO20.byte_slaveID := 5;
		FB_Read_Socomec_IO20.w_Adress := 11621;
		i_State_ModbusCall := i_State_ModbusCall + 1;
	
	61: // wait for received data
		FB_Read_Socomec_IO20.m_Start := FALSE;
		IF FB_Read_Socomec_IO20.iStateModbusRead = 30 THEN
			lr_GasmessungWTTemp_C := LIMIT(-20,100.0/16.0*(FB_Read_Socomec_IO20.lr_Value_mA-4)-20,80);		// 4-20mA = -20-80 °C
				
			i_State_ModbusCall := i_State_ModbusCall +1;
		END_IF            
		IF FB_Read_Socomec_IO20.iStateModbusError = 300 OR ton_TimeOutState.Q THEN
			i_State_ModbusCall := i_State_ModbusCall +1;
		END_IF
	
	62: // wait for FB_Read_Socomec_IO20 to be in state 0
		IF FB_Read_Socomec_IO20.iStateModbusRead = 0 OR ton_TimeOutState.Q THEN
			i_State_ModbusCall := i_State_ModbusCall + 8;
		END_IF
	70:	// next Modbus Poll Call		third IO20 call
		FB_Read_Socomec_IO20.m_Start := TRUE;
		FB_Read_Socomec_IO20.byte_slaveID := 6;
		FB_Read_Socomec_IO20.w_Adress := 11589;
		i_State_ModbusCall := i_State_ModbusCall + 1;
	
	71: // wait for received data
		FB_Read_Socomec_IO20.m_Start := FALSE;
		IF FB_Read_Socomec_IO20.iStateModbusRead = 30 THEN
			lr_GasmessungWTDruck_bar :=	LIMIT(0,(5.0/16.0) * (FB_Read_Socomec_IO20.lr_Value_mA-4),5);		// 4-20mA = 0-5 bar
			i_State_ModbusCall := i_State_ModbusCall +1;
		END_IF            
		IF FB_Read_Socomec_IO20.iStateModbusError = 300 OR ton_TimeOutState.Q THEN
			i_State_ModbusCall := i_State_ModbusCall +1;
		END_IF
	
	72: // wait for FB_Read_Socomec_IO20 to be in state 0
		IF FB_Read_Socomec_IO20.iStateModbusRead = 0 OR ton_TimeOutState.Q THEN
			i_State_ModbusCall := i_State_ModbusCall + 8;
		END_IF

	80:	// next Modbus Poll Call		first IO20 call
