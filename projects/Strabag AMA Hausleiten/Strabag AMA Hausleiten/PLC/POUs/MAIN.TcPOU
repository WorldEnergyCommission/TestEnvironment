	lr_Mischanlage_Gesamt_I2 : LREAL;		//{#lynus.ag#()}
	lr_Mischanlage_Gesamt_I3 : LREAL;		//{#lynus.ag#()}
	lr_Mischanlage_Gesamt_N : LREAL;		//{#lynus.ag#()}
