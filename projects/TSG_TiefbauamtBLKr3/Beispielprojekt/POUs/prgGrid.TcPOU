  <POU Name="prgGrid" Id="{7baeccdf-076f-481e-be2b-3e357576b311}" SpecialFunc="None">
    <Declaration><![CDATA[PROGRAM prgGrid
VAR PERSISTENT
	rSizeGridConn				: REAL;									//{#lynus.ag#()}
END_VAR
VAR
	fbGrid						: FB_Grid_Power_Ext_Input;
	lrGridPower					: LREAL;								//{#lynus.ag#()}
END_VAR
]]></Declaration>
    <Implementation>
      <ST><![CDATA[IF prgEM.fbEM.stDataEMPowerRealTime.byErrorWarning = 2 THEN fbGrid.bExtErrorGrid := TRUE; ELSE fbGrid.bExtErrorGrid := FALSE; END_IF

fbGrid(
	bEnable:= TRUE, 
	diNrOfEMS_IN:= prgEMS.fbEMS.diNrOfEMS_OUT, 
	rSizeGridConn:= rSizeGridConn, 
	bExtErrorGrid:= , 
	lrExtGridPower:= prgEM.fbEM.stDataEMPowerRealTime.lrPowerTotal, 
	lrExtTotalCounterEnergy_Consumption:= 0, 
	lrExtTotalCounterEnergy_Production:= 0, 
	lrExtCounterEnergyT1_Consumption:= 0, 
	lrExtCounterEnergyT2_Consumption:= 0, 
	lrExtCounterEnergyT1_Production:= 0, 
	lrExtCounterEnergyT2_Production:= 0,
	stDataGrid=> ,
	diNrOfGrid_OUT=> );
	
lrGridPower := fbGrid.stDataGrid.lrPower;]]></ST>
    </Implementation>
