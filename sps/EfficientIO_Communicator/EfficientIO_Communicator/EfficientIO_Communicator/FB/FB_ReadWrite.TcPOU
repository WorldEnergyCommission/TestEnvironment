			IF Lynus_Standards.GVL_Communicator.arrInterfaceInfoAlVariables[byReadWriteArrayNumber,udiActiveNumberOfSymbol_ReadAds].uiSizeArray <= Lynus_Standards.Constants_Communicator.bySizeOfValueArray AND iState_ReadAds = 5 THEN 
