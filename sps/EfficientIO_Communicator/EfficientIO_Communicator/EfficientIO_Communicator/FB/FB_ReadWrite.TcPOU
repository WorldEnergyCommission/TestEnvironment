	10://Write in Buffer if the send bus is busy or send a messege if the buffer is not empty
		byCounterWaitInStep_ReadAds := 0;
	
		IF NOT Lynus_Standards.GVL_Communicator.arrInterfaceInfoValueSendTo[byReadWriteArrayNumber].bBusy THEN	
			Lynus_Standards.GVL_Communicator.arrInterfaceInfoValueSendTo[byReadWriteArrayNumber].arrValue := arrValueDefault_ReadAds; 						//Clear send Interface if it is no message to send
				Lynus_Standards.GVL_Communicator.arrInterfaceInfoValueSendTo[byReadWriteArrayNumber].sSymbolUnit := ''; 
