//Data read over ADS

CASE iState_ReadAds OF

	-10://Clear Arrays 
		stADSRead_ReadAds := stADSReadDefault_ReadAds;
			stADSReadNew_ReadAds := stADSReadDefault_ReadAds; 
				bError_ReadAds := FALSE; bFirstReadCycleDone_ReadAds := FALSE; bValueIsTheSame_ReadAds := FALSE;
					uiInternError_ReadAds := 0; udiCounterReadBuffer_New_ReadAds := 0; udiActiveNumberOfSymbol_ReadAds := 0; byCounterWaitInStep_ReadAds := 0; 
						udiNumberInProgress_ReadAds := 0; udiRestValueSymbolRead_ReadAds := 0;
							fbBuffer_ReadAds.A_Reset();
	
		//Clear Interface and lengh from each value
		Lynus_Standards.GVL_Communicator.arrInterfaceInfoValueSendTo[byReadWriteArrayNumber].arrValue := arrValueDefault_ReadAds; Lynus_Standards.GVL_Communicator.arrInterfaceInfoValueSendTo[byReadWriteArrayNumber].sSymbolUnit := ''; Lynus_Standards.GVL_Communicator.arrInterfaceInfoValueSendTo[byReadWriteArrayNumber].sFullSymbolName := '';
			Lynus_Standards.GVL_Communicator.arrInterfaceInfoValueSendTo[byReadWriteArrayNumber].sSymbolType := '';	
					
			iState_ReadAds := -9;
	
	-9://Wait from info symbol function	
		IF NOT Lynus_Standards.GVL_Communicator.bBusyInfoAlVariables AND NOT Lynus_Standards.GVL_Communicator.bErrorInfoAlVariables AND Lynus_Standards.GVL_Communicator.bInfoAlVariablesOK AND NOT Lynus_Standards.GVL_Communicator.arrInterfaceInfoValueSendTo[byReadWriteArrayNumber].bErrorSending AND 
			Lynus_Standards.GVL_Communicator.arrInterfaceInfoValueWriteTo[byReadWriteArrayNumber].bSendFunctionOK THEN 
				iState_ReadAds := -8;
		END_IF
			
	-8: //Init
		uiNumberValueSymbolSize_ReadAds := 0; byNumberOfValues_ReadAds_ReadAds := 0; byLenghSymbolName_ReadAds := 0; byValueInTurn_ReadAds := 0; byCounterValue_ReadAds	:= 0;
			uiValueForLoopStart_ReadAds := 0; uiValueForLoopStop_ReadAds := 0; byCounterValueReadOut_ReadAds := 0;
				fbADSREADWRITE_ReadAds.WRTRD := FALSE;	
		arrCounterWriteValueInArray_ReadAds[1] := 0; arrCounterWriteValueInArray_ReadAds[2] := 0; arrCounterWriteValueInArray_ReadAds[3] := 0; arrCounterWriteValueInArray_ReadAds[4] := 0; arrCounterWriteValueInArray_ReadAds[5] := 0;
			arrCounterWriteValueInArray_ReadAds[6] := 0; arrCounterWriteValueInArray_ReadAds[7] := 0; arrCounterWriteValueInArray_ReadAds[8] := 0;
		arrCounterValueSymbolReads_ReadAds[1] := 0;	arrCounterValueSymbolReads_ReadAds[2] := 0; arrCounterValueSymbolReads_ReadAds[3] := 0; arrCounterValueSymbolReads_ReadAds[4] := 0; arrCounterValueSymbolReads_ReadAds[5] := 0;
			arrCounterValueSymbolReads_ReadAds[6] := 0;	arrCounterValueSymbolReads_ReadAds[7] := 0; arrCounterValueSymbolReads_ReadAds[8] := 0; arrCounterValueSymbolReads_ReadAds[9] := 0; arrCounterValueSymbolReads_ReadAds[10] := 0;									
	
		byCounterBufferReq_ReadAds := 0; uiCounterArrayOfString_ReadAds := 0; uiCounterCopyValueFromReqBuffer_ReadAds := 0; udiStartValueReadOutReqBuffer_AdsRead := 0; udiStopValueReadOutReqBuffer_AdsRead := 0;
		
		sStringFloat_ReadAds := ''; iFindPointFloat_ReadAds := 0; iPrecision_ReadAds := 0;
		
		//Copy first 800 default bytes to save resources on small plc's
		MEMCPY(ADR(arrValueAfterRead_ReadAds),ADR(arrValueAfterReadDefault_ReadAds),SIZEOF(arrValueAfterRead_ReadAds));
		
		//Clear lengh from each value
		arrLenghEachValue_ReadAds := arrLenghEachValueDefault_ReadAds;
			
		//Clear array with symbol name and array with the value names
		arrSymbolName_ReadAds := arrSymbolNameDefault_ReadAds;
			arrValueName_ReadAds := arrValueNameDefault_ReadAds;
			
		//Fill numbers of value after each value is read out
		IF udiRestValueSymbolRead_ReadAds = 0 THEN udiRestValueSymbolRead_ReadAds := Lynus_Standards.GVL_Communicator.arrCounterVariableInArray[byReadWriteArrayNumber]; udiStartValueSymbolsRead_ReadAds := 0; udiStopValueSymbolsRead_ReadAds := 0; END_IF 
	
		IF Lynus_Standards.GVL_Communicator.arrCounterVariableInArray[byReadWriteArrayNumber] > 0 THEN iState_ReadAds := -7; byCounterWaitInStep_ReadAds := 0; END_IF  
		
	-7://Prepeare 10 Symbols to read out
		udiStartValueSymbolsRead_ReadAds := udiStopValueSymbolsRead_ReadAds + 1;
			udiStartValueSymbolsRead_ReadAds := LIMIT(1,udiStartValueSymbolsRead_ReadAds,Lynus_Standards.Constants_Communicator.udiArraySizeMarkedSymobls - 9);
				IF udiRestValueSymbolRead_ReadAds > 10 THEN 
					udiStopValueSymbolsRead_ReadAds := udiStopValueSymbolsRead_ReadAds + 10; 
				ELSIF udiRestValueSymbolRead_ReadAds = 10 THEN
					udiStopValueSymbolsRead_ReadAds := udiStopValueSymbolsRead_ReadAds + udiRestValueSymbolRead_ReadAds;
						udiRestValueSymbolRead_ReadAds := 0;
				ELSIF udiRestValueSymbolRead_ReadAds < 10 THEN
					udiStopValueSymbolsRead_ReadAds := udiStopValueSymbolsRead_ReadAds + udiRestValueSymbolRead_ReadAds;
						udiRestValueSymbolRead_ReadAds := 0;
				END_IF
					IF udiRestValueSymbolRead_ReadAds > 10 THEN udiRestValueSymbolRead_ReadAds := udiRestValueSymbolRead_ReadAds - 10; ELSE udiRestValueSymbolRead_ReadAds := udiRestValueSymbolRead_ReadAds; END_IF 	
		
		udiStopValueSymbolsRead_ReadAds := LIMIT(1,udiStopValueSymbolsRead_ReadAds,Lynus_Standards.Constants_Communicator.udiArraySizeMarkedSymobls);
		
			arrCounterValueSymbolReads_ReadAds[1] := udiStartValueSymbolsRead_ReadAds; arrCounterValueSymbolReads_ReadAds[2] := udiStartValueSymbolsRead_ReadAds + 1; arrCounterValueSymbolReads_ReadAds[3] := udiStartValueSymbolsRead_ReadAds + 2;
				arrCounterValueSymbolReads_ReadAds[4] := udiStartValueSymbolsRead_ReadAds + 3; arrCounterValueSymbolReads_ReadAds[5] := udiStartValueSymbolsRead_ReadAds + 4; arrCounterValueSymbolReads_ReadAds[6] := udiStartValueSymbolsRead_ReadAds + 5;
					arrCounterValueSymbolReads_ReadAds[7] := udiStartValueSymbolsRead_ReadAds + 6; arrCounterValueSymbolReads_ReadAds[8] := udiStartValueSymbolsRead_ReadAds + 7; arrCounterValueSymbolReads_ReadAds[9] := udiStartValueSymbolsRead_ReadAds + 8;
						arrCounterValueSymbolReads_ReadAds[10] := udiStartValueSymbolsRead_ReadAds + 9;
			
			udiNumberSizeValues_ReadAds := (udiStopValueSymbolsRead_ReadAds - udiStartValueSymbolsRead_ReadAds + 1);			
			
			FOR byLPClearStructs_ReadAds := 1 TO 10 BY 1 DO
				arrInfoValue_Read_Ads[byLPClearStructs_ReadAds].udiIndexGroup := 0;
				arrInfoValue_Read_Ads[byLPClearStructs_ReadAds].udiIndexOffset := 0;
				arrInfoValue_Read_Ads[byLPClearStructs_ReadAds].udiLength := 0;
			END_FOR
			
				iState_ReadAds := -6;

	-6://Prepeare structs
		arrInfoValue_Read_Ads[1].udiIndexGroup := Lynus_Standards.GVL_Communicator.arrInterfaceInfoAlVariables[byReadWriteArrayNumber,arrCounterValueSymbolReads_ReadAds[1]].udiGroup;
		arrInfoValue_Read_Ads[1].udiIndexOffset := Lynus_Standards.GVL_Communicator.arrInterfaceInfoAlVariables[byReadWriteArrayNumber,arrCounterValueSymbolReads_ReadAds[1]].udiOffset;
		arrInfoValue_Read_Ads[1].udiLength := Lynus_Standards.GVL_Communicator.arrInterfaceInfoAlVariables[byReadWriteArrayNumber,arrCounterValueSymbolReads_ReadAds[1]].udiSize;
			arrInfoValue_Read_Ads[2].udiIndexGroup := Lynus_Standards.GVL_Communicator.arrInterfaceInfoAlVariables[byReadWriteArrayNumber,arrCounterValueSymbolReads_ReadAds[2]].udiGroup;
			arrInfoValue_Read_Ads[2].udiIndexOffset := Lynus_Standards.GVL_Communicator.arrInterfaceInfoAlVariables[byReadWriteArrayNumber,arrCounterValueSymbolReads_ReadAds[2]].udiOffset;
			arrInfoValue_Read_Ads[2].udiLength := Lynus_Standards.GVL_Communicator.arrInterfaceInfoAlVariables[byReadWriteArrayNumber,arrCounterValueSymbolReads_ReadAds[2]].udiSize;
		arrInfoValue_Read_Ads[3].udiIndexGroup := Lynus_Standards.GVL_Communicator.arrInterfaceInfoAlVariables[byReadWriteArrayNumber,arrCounterValueSymbolReads_ReadAds[3]].udiGroup;
		arrInfoValue_Read_Ads[3].udiIndexOffset := Lynus_Standards.GVL_Communicator.arrInterfaceInfoAlVariables[byReadWriteArrayNumber,arrCounterValueSymbolReads_ReadAds[3]].udiOffset;
		arrInfoValue_Read_Ads[3].udiLength := Lynus_Standards.GVL_Communicator.arrInterfaceInfoAlVariables[byReadWriteArrayNumber,arrCounterValueSymbolReads_ReadAds[3]].udiSize;
