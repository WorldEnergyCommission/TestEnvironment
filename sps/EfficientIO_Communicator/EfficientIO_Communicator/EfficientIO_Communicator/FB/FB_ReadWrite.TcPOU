										arrValueOutFromBuffer_ReadAds[byCounterValueReadOut_ReadAds] := CONCAT(arrValueOutFromBuffer_ReadAds[byCounterValueReadOut_ReadAds],F_ToCHR(arrBufferRead_ReadAds[udiLPCheckReadBuffer_ReadAds])); 
									END_IF    	
					END_FOR				 
						IF arrBufferRead_ReadAds[udiLPCheckReadBuffer_ReadAds] = 0 THEN iState_ReadAds := 12; byCounterValueReadOut_ReadAds := 0; END_IF
				END_IF
		END_IF
		
			IF timTimout_ReadAds.Q THEN iState_ReadAds := 200; END_IF
	
	12://Write the Date on interface and start the sending
		byCounterWaitInStep_ReadAds := byCounterWaitInStep_ReadAds + 1;
			byCounterWaitInStep_ReadAds := LIMIT(1,byCounterWaitInStep_ReadAds,2);
	
		IF byCounterWaitInStep_ReadAds = 1 THEN 
			FOR udiLPCheckVarInfoBeforSending_ReadAds := 1 TO udiNumberLoopHalf_ReadAds BY 1 DO
				IF Lynus_Standards.GVL_Communicator.arrInterfaceInfoAlVariables[byReadWriteArrayNumber,udiLPCheckVarInfoBeforSending_ReadAds].sFullSymbolName = sSymbolNameOutFromBuffer_ReadAds THEN
					Lynus_Standards.GVL_Communicator.arrInterfaceInfoValueSendTo[byReadWriteArrayNumber].sFullSymbolName := sSymbolNameOutFromBuffer_ReadAds;
						Lynus_Standards.GVL_Communicator.arrInterfaceInfoValueSendTo[byReadWriteArrayNumber].sSymbolUnit := Lynus_Standards.GVL_Communicator.arrInterfaceInfoAlVariables[byReadWriteArrayNumber,udiLPCheckVarInfoBeforSending_ReadAds].sSymbolUnit;
							Lynus_Standards.GVL_Communicator.arrInterfaceInfoValueSendTo[byReadWriteArrayNumber].sSymbolType := Lynus_Standards.GVL_Communicator.arrInterfaceInfoAlVariables[byReadWriteArrayNumber,udiLPCheckVarInfoBeforSending_ReadAds].sSymbolType;
								Lynus_Standards.GVL_Communicator.arrInterfaceInfoValueSendTo[byReadWriteArrayNumber].arrValue := arrValueOutFromBuffer_ReadAds;
									Lynus_Standards.GVL_Communicator.arrInterfaceInfoValueSendTo[byReadWriteArrayNumber].bBusy := TRUE;
										iState_ReadAds := 13;
											EXIT;	
				END_IF 		 	   	
			END_FOR 	
		END_IF
			IF byCounterWaitInStep_ReadAds = 2 AND udiNumberLoopFull_ReadAds <> udiNumberLoopHalf_ReadAds THEN 
				FOR udiLPCheckVarInfoBeforSending_ReadAds := (udiNumberLoopHalf_ReadAds + 1) TO udiNumberLoopFull_ReadAds BY 1 DO
					IF Lynus_Standards.GVL_Communicator.arrInterfaceInfoAlVariables[byReadWriteArrayNumber,udiLPCheckVarInfoBeforSending_ReadAds].sFullSymbolName = sSymbolNameOutFromBuffer_ReadAds THEN
						Lynus_Standards.GVL_Communicator.arrInterfaceInfoValueSendTo[byReadWriteArrayNumber].sFullSymbolName := sSymbolNameOutFromBuffer_ReadAds;
							Lynus_Standards.GVL_Communicator.arrInterfaceInfoValueSendTo[byReadWriteArrayNumber].sSymbolUnit := Lynus_Standards.GVL_Communicator.arrInterfaceInfoAlVariables[byReadWriteArrayNumber,udiLPCheckVarInfoBeforSending_ReadAds].sSymbolUnit;
								Lynus_Standards.GVL_Communicator.arrInterfaceInfoValueSendTo[byReadWriteArrayNumber].sSymbolType := Lynus_Standards.GVL_Communicator.arrInterfaceInfoAlVariables[byReadWriteArrayNumber,udiLPCheckVarInfoBeforSending_ReadAds].sSymbolType;
									Lynus_Standards.GVL_Communicator.arrInterfaceInfoValueSendTo[byReadWriteArrayNumber].arrValue := arrValueOutFromBuffer_ReadAds;
										Lynus_Standards.GVL_Communicator.arrInterfaceInfoValueSendTo[byReadWriteArrayNumber].bBusy := TRUE;
											iState_ReadAds := 13;
												EXIT;	
					END_IF 		 	   	
				END_FOR 	
			END_IF
		
			IF timTimout_ReadAds.Q THEN iState_ReadAds := 200; END_IF
			
	13://Clear Read Buffer (conserve resources because the code must also work for small plc's)
		byCounterWaitInStep_ReadAds := 0;
			sSymbolNameOutFromBuffer_ReadAds := '';
				MEMCPY(ADR(arrBufferRead_ReadAds),ADR(arrBufferWriteReadDefault_ReadAds),SIZEOF(arrBufferRead_ReadAds));
					arrValueOutFromBuffer_ReadAds := arrValueDefault_ReadAds;
		
			iState_ReadAds := 10;
			
	20://Dim size from the values and symbol name befor can saving in buffer
		uiNumberValueSymbolSize_ReadAds := 0;
			byNumberOfValues_ReadAds_ReadAds := 0;
				byLenghSymbolName_ReadAds := 0;
		FOR byLPDimValueNumberASize_ReadAds := 1 TO Lynus_Standards.Constants_Communicator.bySizeOfValueArray BY 1 DO
			uiNumberValueSymbolSize_ReadAds := uiNumberValueSymbolSize_ReadAds + INT_TO_UINT(LEN(stADSReadNew_ReadAds[udiActiveNumberOfSymbol_ReadAds].arrValue[byLPDimValueNumberASize_ReadAds]));
				IF stADSReadNew_ReadAds[udiActiveNumberOfSymbol_ReadAds].arrValue[byLPDimValueNumberASize_ReadAds] <> '' THEN uiNumberValueSymbolSize_ReadAds := uiNumberValueSymbolSize_ReadAds + 1; END_IF 	
					IF stADSReadNew_ReadAds[udiActiveNumberOfSymbol_ReadAds].arrValue[byLPDimValueNumberASize_ReadAds] <> '' THEN byNumberOfValues_ReadAds_ReadAds := byNumberOfValues_ReadAds_ReadAds + 1; END_IF	
		END_FOR 	
			byLenghSymbolName_ReadAds := INT_TO_BYTE(LEN(stADSReadNew_ReadAds[udiActiveNumberOfSymbol_ReadAds].sFullSymbolName));
				uiNumberValueSymbolSize_ReadAds := uiNumberValueSymbolSize_ReadAds + byLenghSymbolName_ReadAds + 1;
		
			IF byNumberOfValues_ReadAds_ReadAds <= Lynus_Standards.Constants_Communicator.bySizeOfValueArray THEN
				FOR byLPDimLenghValues_ReadAds := 1 TO byNumberOfValues_ReadAds_ReadAds BY 1 DO
					arrLenghEachValue_ReadAds[byLPDimLenghValues_ReadAds] := INT_TO_BYTE(LEN(stADSReadNew_ReadAds[udiActiveNumberOfSymbol_ReadAds].arrValue[byLPDimLenghValues_ReadAds]));	
				END_FOR 
			END_IF
	
		IF byNumberOfValues_ReadAds_ReadAds <= Lynus_Standards.Constants_Communicator.bySizeOfValueArray AND byNumberOfValues_ReadAds_ReadAds >= 1 AND uiNumberValueSymbolSize_ReadAds <> 0 THEN iState_ReadAds := 21; END_IF
		
			IF timTimout_ReadAds.Q THEN iState_ReadAds := 200; END_IF
			
	21://String to Ascii Symbol Name and values
		IF byLenghSymbolName_ReadAds <= 49 THEN
			FOR byLPSymbolNameToAscii_ReadAds := 1 TO byLenghSymbolName_ReadAds BY 1 DO
				arrSymbolName_ReadAds[byLPSymbolNameToAscii_ReadAds] := F_ToASC(MID(stADSReadNew_ReadAds[udiActiveNumberOfSymbol_ReadAds].sFullSymbolName,1,byLPSymbolNameToAscii_ReadAds));
			END_FOR 
		END_IF
			IF byLenghSymbolName_ReadAds < 49 THEN arrSymbolName_ReadAds[byLenghSymbolName_ReadAds + 1] := F_ToASC(';'); END_IF  	
		
		IF byNumberOfValues_ReadAds_ReadAds <= Lynus_Standards.Constants_Communicator.bySizeOfValueArray THEN 
			FOR byLPValueNameToAscii_1_ReadAds := 1 TO byNumberOfValues_ReadAds_ReadAds BY 1 DO
				IF arrLenghEachValue_ReadAds[byLPValueNameToAscii_1_ReadAds] <= 31 THEN 
					FOR byLPValueNameToAscii_2_ReadAds := 1 TO arrLenghEachValue_ReadAds[byLPValueNameToAscii_1_ReadAds] BY 1 DO
						arrValueName_ReadAds[byLPValueNameToAscii_1_ReadAds,byLPValueNameToAscii_2_ReadAds] := F_ToASC(MID(stADSReadNew_ReadAds[udiActiveNumberOfSymbol_ReadAds].arrValue[byLPValueNameToAscii_1_ReadAds],1,byLPValueNameToAscii_2_ReadAds));
					END_FOR
				END_IF
					arrValueName_ReadAds[byLPValueNameToAscii_1_ReadAds,byLPValueNameToAscii_2_ReadAds] := F_ToASC(';'); 			
			END_FOR 
		END_IF 
		
		IF byLenghSymbolName_ReadAds <= 49 AND byNumberOfValues_ReadAds_ReadAds <= Lynus_Standards.Constants_Communicator.bySizeOfValueArray AND arrLenghEachValue_ReadAds[byNumberOfValues_ReadAds_ReadAds] <= 31 THEN iState_ReadAds := 22; END_IF
		
			IF timTimout_ReadAds.Q THEN iState_ReadAds := 200; END_IF	
			
	22://Prepare Write array for Buffer
		IF byLenghSymbolName_ReadAds <= 48 THEN
			FOR uiLPWriteByteValueInArray_ReadAds := 1 TO (byLenghSymbolName_ReadAds + 1) BY 1 DO
				arrBufferWrite_ReadAds[uiLPWriteByteValueInArray_ReadAds] := arrSymbolName_ReadAds[uiLPWriteByteValueInArray_ReadAds];
			END_FOR 
		END_IF
		
		IF byLenghSymbolName_ReadAds <= 48 THEN	iState_ReadAds := 23; END_IF
			
			IF timTimout_ReadAds.Q THEN iState_ReadAds := 200; END_IF
	
	23://Prepare Write array for Buffer
		uiValueForLoopStart_ReadAds := uiLPWriteByteValueInArray_ReadAds;
			byValueInTurn_ReadAds := byValueInTurn_ReadAds + 1;
				byValueInTurn_ReadAds := LIMIT(1,byValueInTurn_ReadAds,Lynus_Standards.Constants_Communicator.bySizeOfValueArray);
					uiValueForLoopStop_ReadAds := arrLenghEachValue_ReadAds[byValueInTurn_ReadAds] + uiValueForLoopStart_ReadAds;
						byCounterValue_ReadAds := 0;
							
							IF uiValueForLoopStart_ReadAds >= 1 AND uiValueForLoopStart_ReadAds <= 2000 AND uiValueForLoopStop_ReadAds >= 1 AND uiValueForLoopStop_ReadAds <= 2000 THEN 
								FOR uiLPWriteByteValueInArray_ReadAds := uiValueForLoopStart_ReadAds TO uiValueForLoopStop_ReadAds BY 1 DO
									byCounterValue_ReadAds :=  byCounterValue_ReadAds + 1;
										byCounterValue_ReadAds := LIMIT(1,byCounterValue_ReadAds,31);	
											arrBufferWrite_ReadAds[uiLPWriteByteValueInArray_ReadAds] := arrValueName_ReadAds[byValueInTurn_ReadAds,byCounterValue_ReadAds];
								END_FOR 
							END_IF
		
		IF byValueInTurn_ReadAds >= byNumberOfValues_ReadAds_ReadAds AND uiValueForLoopStart_ReadAds >= 1 AND uiValueForLoopStart_ReadAds <= 2000 AND uiValueForLoopStop_ReadAds >= 1 AND uiValueForLoopStop_ReadAds <= 2000 THEN 
			iState_ReadAds := 24; 
				uiValueForLoopStart_ReadAds := 0;
					byValueInTurn_ReadAds := 0;
						uiValueForLoopStop_ReadAds := 0;
							byCounterValue_ReadAds := 0;			
		END_IF
			
			IF timTimout_ReadAds.Q THEN iState_ReadAds := 200; END_IF

	24://Write Value in Buffer
		iState_ReadAds := 25; 
				
	25://Check if the new value is stored in the buffer
		IF fbBuffer_ReadAds.bOk AND fbBuffer_ReadAds.nCount <> udiCounterReadBuffer_New_ReadAds THEN udiCounterReadBuffer_New_ReadAds := fbBuffer_ReadAds.nCount; iState_ReadAds := 26; END_IF  	
			
			IF timTimout_ReadAds.Q THEN iState_ReadAds := 200; END_IF
		
	26://Clear the Write Buffer (conserve resources because the code must also work for small plc's)
		MEMCPY(ADR(arrBufferWrite_ReadAds),ADR(arrBufferWriteReadDefault_ReadAds),SIZEOF(arrBufferWrite_ReadAds));
		
			iState_ReadAds := 100;

	100://Check for other variable
		//Next symbol
		IF udiActiveNumberOfSymbol_ReadAds < Lynus_Standards.GVL_Communicator.arrCounterVariableInArray[byReadWriteArrayNumber] AND udiNumberInProgress_ReadAds < udiNumberSizeValues_ReadAds THEN iState_ReadAds := -4; udiNumberInProgress_ReadAds := udiNumberInProgress_ReadAds + 1; END_IF
			//Start with the next 10 Variables
			IF (udiNumberInProgress_ReadAds >= udiNumberSizeValues_ReadAds AND iState_ReadAds = 100) THEN 
				iState_ReadAds := -8; 
					udiNumberInProgress_ReadAds := 0;
			END_IF	
				//Start from New with the first 10 Variables 	
				IF udiActiveNumberOfSymbol_ReadAds >= Lynus_Standards.GVL_Communicator.arrCounterVariableInArray[byReadWriteArrayNumber] THEN
					iState_ReadAds := -8; 
						udiActiveNumberOfSymbol_ReadAds := 0;
							udiNumberInProgress_ReadAds := 0;
								udiRestValueSymbolRead_ReadAds := 0;
						 			bFirstReadCycleDone_ReadAds := TRUE; 
				END_IF			
			
			bError_ReadAds := FALSE;
				uiInternError_ReadAds := 0;
			
			IF timTimout_ReadAds.Q THEN iState_ReadAds := 200; END_IF
				
	200://Error
		bError_ReadAds := TRUE; fbADSREADWRITE_ReadAds.WRTRD := FALSE; bValueIsTheSame_ReadAds := FALSE; 
			udiActiveNumberOfSymbol_ReadAds := 0; byCounterWaitInStep_ReadAds := 0; udiNumberInProgress_ReadAds := 0; udiRestValueSymbolRead_ReadAds := 0;
		
		//Clear the write Buffer befor go to step 0 (conserve resources because the code must also work for small plc's)
		MEMCPY(ADR(arrBufferWrite_ReadAds),ADR(arrBufferWriteReadDefault_ReadAds),SIZEOF(arrBufferWrite_ReadAds));
		
		//Clear the read Buffer befor go to step 0 (conserve resources because the code must also work for small plc's)
		sSymbolNameOutFromBuffer_ReadAds := '';
			MEMCPY(ADR(arrBufferRead_ReadAds),ADR(arrBufferWriteReadDefault_ReadAds),SIZEOF(arrBufferRead_ReadAds));
				arrValueOutFromBuffer_ReadAds := arrValueDefault_ReadAds;
			
		//Count up when an error has occurred
		byErrorCounter_ReadAds := byErrorCounter_ReadAds + 1;		
				
			iState_ReadAds := 300;
	
	300://Wait in Error and clear the rest from the read and write buffer (conserve resources because the code must also work for small plc's)
		byCounterWaitInStep_ReadAds := byCounterWaitInStep_ReadAds + 1;
	
			//Restart new from in Step -8. No Reset from the complete System
			IF byCounterWaitInStep_ReadAds > 10 AND byErrorCounter_ReadAds <= 10 THEN byCounterWaitInStep_ReadAds := 0; iState_ReadAds := -8; END_IF
				//Restart new from in Step -10. Reset from the complete System after to much Errors
				IF byCounterWaitInStep_ReadAds > 10 AND byErrorCounter_ReadAds > 10 THEN 
						byCounterWaitInStep_ReadAds := 0; 
							//Reset Read Steps and start from new
							iState_ReadAds := -10;
								//Reset Write Steps and start from new
								iState_WriteAds := -2;
									byErrorCounter_ReadAds := 0; 
				END_IF
	
END_CASE	

//Resetting the error counter after not being in the error step for 30 min
timResetErrorCounter_ReadAds(IN:= byErrorCounter_ReadAds <> 0, PT:= T#30M, Q=> , ET=> );
	IF timResetErrorCounter_ReadAds.Q THEN byErrorCounter_ReadAds := 0; END_IF 

//Read Out Variable by ADS
fbADSREADWRITE_ReadAds(
	NETID:= fbNedID.AddrString, 
	PORT:= uiPort, 
	IDXGRP:= 16#F080, 
	IDXOFFS:= udiNumberSizeValues_ReadAds, 
	WRITELEN:= , 
	READLEN:= udiSizeReq_ReadAds, 
	SRCADDR:= , 
	DESTADDR:= ADR(arrValueRead_ReadAds), 
	WRTRD:= , 
	TMOUT:= T#5S, 
	BUSY=> , 
	ERR=> , 
	ERRID=> );

FPErrorReadWrite_ReadAds(CLK:= fbADSREADWRITE_ReadAds.ERR, Q=> );	
	
//Buffer Read Value from ADS
FPWriteValueToBufferStep25_ReadAds(CLK:= iState_ReadAds = 24, Q=> );
FPReadValueToBufferStep11_ReadAds(CLK:= iState_ReadAds = 11, Q=> );

IF FPReadValueToBufferStep11_ReadAds.Q THEN fbBuffer_ReadAds.A_RemoveHead(); END_IF
IF FPWriteValueToBufferStep25_ReadAds.Q THEN fbBuffer_ReadAds.A_AddTail(); END_IF

//Reset the Buffer after a Buffer Overflow
IF fbBuffer_ReadAds.nCount <> 0 AND NOT fbBuffer_ReadAds.bOk THEN timResetBuffer_ReadAds.IN := TRUE; ELSE timResetBuffer_ReadAds.IN := FALSE; END_IF
	timResetBuffer_ReadAds(IN:= , PT:= T#40S, Q=> , ET=> );
		IF timResetBuffer_ReadAds.Q THEN fbBuffer_ReadAds.A_Reset(); END_IF

fbBuffer_ReadAds(
	pWrite:= ADR(arrBufferWrite_ReadAds), 
	cbWrite:= uiNumberValueSymbolSize_ReadAds, 
	pRead:= ADR(arrBufferRead_ReadAds), 
	cbRead:= SIZEOF(arrBufferRead_ReadAds), 
	pBuffer:= ADR(arrBufferADSRead_ReadAds), 
	cbBuffer:= SIZEOF(arrBufferADSRead_ReadAds), 
	bOk=> Lynus_Standards.GVL_Communicator.arrInterfaceInfoValueSendTo[byReadWriteArrayNumber].bBufferOK, 
	nCount=> Lynus_Standards.GVL_Communicator.arrInterfaceInfoValueSendTo[byReadWriteArrayNumber].udiCountBuffer, 
	cbSize=> , 
	cbReturn=> );

END_IF
	
//Busy
IF iState_ReadAds >= -8 OR iState_WriteAds >= 0 THEN bBusy := TRUE; ELSE bBusy := FALSE; END_IF
	
//Error
IF (bError_ReadAds AND iState_ReadAds <> - 10 AND iState_ReadAds <> - 9) OR (bError_WriteAds AND iState_WriteAds <> - 1 AND iState_WriteAds <> - 2) THEN fbErrorCodes.bErrorIntern := TRUE; ELSE fbErrorCodes.bErrorIntern := FALSE; END_IF

IF fbADSREADWRITE_ReadAds.ERR OR fbADSREADWRITE_WriteAds.ERR OR fbADSWrite_WriteAds.ERR THEN fbErrorCodes.bErrorADS := TRUE; END_IF

IF fbADSREADWRITE_ReadAds.ERR THEN fbErrorCodes.udiErrorCodeADS := fbADSREADWRITE_ReadAds.ERRID; END_IF
IF fbADSREADWRITE_WriteAds.ERR THEN fbErrorCodes.udiErrorCodeADS := fbADSREADWRITE_WriteAds.ERRID; END_IF
IF fbADSWrite_WriteAds.ERR THEN fbErrorCodes.udiErrorCodeADS := fbADSWrite_WriteAds.ERRID; END_IF

IF NOT fbADSREADWRITE_ReadAds.ERR AND NOT fbADSREADWRITE_ReadAds.BUSY AND NOT
	fbADSREADWRITE_WriteAds.ERR AND NOT fbADSREADWRITE_WriteAds.BUSY AND NOT fbADSWrite_WriteAds.ERR AND NOT fbADSWrite_WriteAds.BUSY THEN 
		fbErrorCodes.bErrorADS := FALSE;
			fbErrorCodes.udiErrorCodeADS := 0; 
END_IF 	     
	
fbErrorCodes(
	bErrorADS:= , 
	bErrorIntern:= , 
	udiErrorCodeADS:= , 
	udiErrorCodesInternRead:= uiInternError_ReadAds, 
	udiErrorCodesInternWrite:= uiInternError_WriteAds, 
	udiErrorCodeADS_Out=> udiErrorCodeOut, 
	bError=> bError, 
	eErrorReadWrite_Out=> eErrorReadWrite_Out);]]></ST>
