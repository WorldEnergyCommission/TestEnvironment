			ELSE		
				IF iState_ReadAds = 7 AND byLPCheckValueChange_ReadAds >= Lynus_Standards.Constants_Communicator.bySizeOfValueArray THEN iState_ReadAds := 10; bValueIsTheSame_ReadAds := TRUE; END_IF		//Value is the same
			END_IF 
		END_FOR	
		
			//Timeout
			IF timTimout_ReadAds.Q THEN iState_ReadAds := 200; END_IF	
		
	8://Symbol exist and Value has changed
		byCounterWaitInStep_ReadAds := byCounterWaitInStep_ReadAds + 1;
			byCounterWaitInStep_ReadAds := LIMIT(1,byCounterWaitInStep_ReadAds,2);
	
		IF byCounterWaitInStep_ReadAds = 1 THEN 
