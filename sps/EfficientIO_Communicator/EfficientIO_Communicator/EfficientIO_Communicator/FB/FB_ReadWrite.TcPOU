	unConvertDWORD_TO_REAL_ReadAds							: ST_UN_FLOAT_CONVERT_DWORD_REAL;																	//Union to convert numbers
	unConvertULINT_TO_LREAL_ReadAds							: ST_UN_FLOAT_CONVERT_ULINT_LREAL;																	//Union to convert numbers
	fbBuffer_ReadAds										: FB_MemRingBuffer;																					//Ringbuffer read
	FPErrorReadWrite_ReadAds								: R_TRIG;																							//Internal rising edge 
	FPWriteValueToBufferStep25_ReadAds						: R_TRIG;																							//Internal rising edge 
	FPReadValueToBufferStep11_ReadAds						: R_TRIG;																							//Internal rising edge 
