					IF stADSReadNew_ReadAds[udiLPCheckReadArrays_ReadAds].sFullSymbolName = Lynus_Standards.GVL_Communicator.arrInterfaceInfoAlVariables[byReadWriteArrayNumber,udiActiveNumberOfSymbol_ReadAds].sFullSymbolName THEN 
						iState_ReadAds := 7;																			//Symbol exist in the read array
					END_IF
				END_FOR
			END_IF
		
				IF byCounterWaitInStep_ReadAds >= 2 AND iState_ReadAds = 6 THEN iState_ReadAds := 9; END_IF 			//Symbol not exist	
				
			//Timeout
			IF timTimout_ReadAds.Q THEN iState_ReadAds := 200; END_IF		
				
	7://Check if the value has changed
		byCounterWaitInStep_ReadAds := 0;
