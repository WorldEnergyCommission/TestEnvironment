						Lynus_Standards.GVL_Communicator.arrInterfaceInfoValueSendTo[byReadWriteArrayNumber].sSymbolType := '';
							IF fbBuffer_ReadAds.nCount > 0 THEN							
								iState_ReadAds := 11;														//Send a new messege from the buffer
							END_IF																			
		END_IF
			IF ((Lynus_Standards.GVL_Communicator.arrInterfaceInfoValueSendTo[byReadWriteArrayNumber].bBusy OR fbBuffer_ReadAds.nCount = 0) AND NOT bValueIsTheSame_ReadAds) THEN
