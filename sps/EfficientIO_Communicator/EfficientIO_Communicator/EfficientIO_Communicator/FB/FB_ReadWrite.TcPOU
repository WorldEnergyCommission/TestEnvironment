			ELSE
				IF iState_ReadAds = 10 THEN 																						
					iState_ReadAds := 100;																	//Value is the same
				END_IF																						
			END_IF
		
			//Timeout
			IF timTimout_ReadAds.Q THEN iState_ReadAds := 200; END_IF
	
	11://Read out a Value from the buffer for sending
		IF udiCounterReadBuffer_New_ReadAds <> fbBuffer_ReadAds.nCount AND fbBuffer_ReadAds.bOk THEN
			udiCounterReadBuffer_New_ReadAds := fbBuffer_ReadAds.nCount;
				IF fbBuffer_ReadAds.cbReturn <= 2000 THEN 
					FOR udiLPCheckReadBuffer_ReadAds := 1 TO fbBuffer_ReadAds.cbReturn BY 1 DO	
						IF byCounterValueReadOut_ReadAds = 0 AND arrBufferRead_ReadAds[udiLPCheckReadBuffer_ReadAds] <> 59 THEN sSymbolNameOutFromBuffer_ReadAds := CONCAT(sSymbolNameOutFromBuffer_ReadAds,F_ToCHR(arrBufferRead_ReadAds[udiLPCheckReadBuffer_ReadAds])); END_IF 
