	timResetBuffer_ReadAds									: TON;																								//Timer to Reset the Buffer after a Buffer Overflow
	timResetErrorCounter_ReadAds							: TON;																								//Timer to Reset the Error Counter when a longer Time we dont go inside the Error State
	stADSReadDefault_ReadAds								: ARRAY[1..Lynus_Standards.Constants_Communicator.udiArraySizeMarkedSymobls] OF ST_SymbolRead;		//Array with the default Data of al matching variables
	stADSRead_ReadAds										: ARRAY[1..Lynus_Standards.Constants_Communicator.udiArraySizeMarkedSymobls] OF ST_SymbolRead;		//Array with the Data of al matching variables
	stADSReadNew_ReadAds									: ARRAY[1..Lynus_Standards.Constants_Communicator.udiArraySizeMarkedSymobls] OF ST_SymbolRead;		//Array with the Data of al matching variables
	arrInfoValue_Read_Ads									: ARRAY[1..10] OF ST_BlockReadValue;																//Struct with symbol values
	arrValueAfterRead_ReadAds								: ARRAY[1..1600] OF BYTE;																			//Buffer after reat out data
	arrValueAfterReadDefault_ReadAds						: ARRAY[1..1600] OF BYTE;																			//Buffer after reat out data default
	arrValueRead_ReadAds									: ARRAY[1..16000] OF BYTE;																			//Buffer size for read function
	arrSymbolName_ReadAds									: ARRAY[1..49] OF BYTE;																				//Smybol name in single chracters	
	arrSymbolNameDefault_ReadAds							: ARRAY[1..49] OF BYTE;																				//Default Smybol name in single chracters	
	arrValueName_ReadAds									: ARRAY[1..Lynus_Standards.Constants_Communicator.bySizeOfValueArray,1..31] OF BYTE;				//Value name in single chracters	
	arrValueNameDefault_ReadAds								: ARRAY[1..Lynus_Standards.Constants_Communicator.bySizeOfValueArray,1..31] OF BYTE;				//Default Value name in single chracters 	
	arrLenghEachValue_ReadAds								: ARRAY[1..Lynus_Standards.Constants_Communicator.bySizeOfValueArray] OF BYTE; 						//Lengt of each value
	arrLenghEachValueDefault_ReadAds						: ARRAY[1..Lynus_Standards.Constants_Communicator.bySizeOfValueArray] OF BYTE; 						//Default Lengt of each value 	
	arrBufferADSRead_ReadAds								: ARRAY[1..32000] OF BYTE;																			//Buffer read size
	arrBufferRead_ReadAds									: ARRAY[1..2000] OF BYTE;																			//Value read out from send buffer
	arrBufferWrite_ReadAds									: ARRAY[1..2000] OF BYTE;																			//Value to write in buffer
	arrBufferWriteReadDefault_ReadAds						: ARRAY[1..2000] OF BYTE;																			//Default buffer for read and write 
	arrReqValue_ReadAds										: ARRAY[1..10,1..12] OF BYTE;																		//Data for Block Request
	arrBufferReq_ReadAds									: ARRAY[1..120] OF BYTE;																			//Data for Block Request
	arrCounterWriteValueInArray_ReadAds						: ARRAY[1..8] OF UINT;																				//Counter to write the read value if it is a array
	arrValueDefault_ReadAds									: ARRAY[1..Lynus_Standards.Constants_Communicator.bySizeOfValueArray] OF STRING(30);				//Default Value from the Variable
	arrValueOutFromBuffer_ReadAds							: ARRAY[1..Lynus_Standards.Constants_Communicator.bySizeOfValueArray] OF STRING(30);				//Values from read Buffer
	arrCounterValueSymbolReads_ReadAds						: ARRAY[1..10] OF UDINT;																			//Counter to read out symbols
	sSymbolNameOutFromBuffer_ReadAds						: STRING(48);																						//Symbol Name from read Buffer
	bValueIsTheSame_ReadAds									: BOOL;																								//True Symbol has the same value; False Symbol has changed the value
	bError_ReadAds											: BOOL;																								//Error flag
	bFirstReadCycleDone_ReadAds								: BOOL;																								//True if the first read cycle is done. Befor write value is deactivated
	byErrorCounter_ReadAds									: BYTE;																								//Counter entryes in the Error State
	byCounterWaitInStep_ReadAds								: BYTE;																								//Wait counter in statemschine
	byLPDimValueNumberASize_ReadAds							: BYTE;																								//Loop to dim numer of values and the size
	byLPSymbolNameToAscii_ReadAds							: BYTE;																								//Loop to write the symbol name as asci in the array
	byLPValueNameToAscii_1_ReadAds							: BYTE;																								//Loop to write the value name as asci in the array
	byLPValueNameToAscii_2_ReadAds							: BYTE;																								//Loop to write the value name as asci in the array
	byLPDimLenghValues_ReadAds								: BYTE;																								//Loop to dim lengh of the values
	byLPCheckValueChange_ReadAds							: BYTE;																								//Loop to check the value change
	byLPCheckCharactersAfterReadOut_ReadAds					: BYTE;																								//Loop to convert the Byte after read out in Characters
	byLPClearStructs_ReadAds								: BYTE;																								//Loop to clear structs
	byLPFillBufferReq_1_ReadAds								: BYTE;																								//Loop to fill the request buffer
	byLPFillBufferReq_2_ReadAds								: BYTE;																								//Loop to fill the request buffer
	byNumberOfValues_ReadAds_ReadAds						: BYTE;																								//Number of values befor saving in the buffer
	byLenghSymbolName_ReadAds								: BYTE;																								//Size of the symbol name befor saving
	byValueInTurn_ReadAds									: BYTE;																								//Value in turn when filling the write array
	byCounterValue_ReadAds									: BYTE;																								//Value currently being written into the array
	byCounterValueReadOut_ReadAds							: BYTE;																								//Counter to encode the read out Data from buffer
	byCounterBufferReq_ReadAds								: BYTE;																								//Counter to fill the request buffer																					
	iState_ReadAds											: INT := -2;																						//Statemaschine for ADS Read
	iFindPointFloat_ReadAds									: INT;																								//Variable to find the point in the float value
	iPrecision_ReadAds										: INT;																								//Precision for float value
	uiNumberValueSymbolSize_ReadAds							: UINT;																								//Size of the complete charachters befror store in the buffer
	uiLPWriteValueByArray_ReadAds							: UINT;																								//Loop to write the values from the read ads data in the array of values
	uiLPWriteByteValueInArray_ReadAds						: UINT;																								//Loop to write the values in the write array
	uiValueForLoopStart_ReadAds								: UINT;																								//Value start for loop write value in write array
	uiValueForLoopStop_ReadAds								: UINT;																								//Value stop for loop write value in write array
	uiInternError_ReadAds									: UINT;																								//Intern Error Code Read ads
	uiCounterArrayOfString_ReadAds							: UINT;																								//Counter to encode the array form the read value
	uiCounterCopyValueFromReqBuffer_ReadAds					: UINT;																								//Conter to copy the value from big intern buffer to small buffer
	udiLPCheckReadArrays_ReadAds							: UDINT;																							//Loop to check the read array for name and value
	udiLPCheckReadBuffer_ReadAds							: UDINT;																							//Loop to check the symbols in the read buffer
	udiLPCheckVarInfoBeforSending_ReadAds					: UDINT;																							//Loop to check the var info befor sending
	udiLPCheckErrorAfterReadReq_ReadAds						: UDINT;																							//Loop to check Error after read out
	udiLPCopyValueFromReqBuffer_ReadAds						: UDINT;																							//Loop to copy the values from request buffer
	udiActiveNumberOfSymbol_ReadAds							: UDINT;																							//Number of the Varaibele that is currently being read out
	udiCounterReadBuffer_New_ReadAds						: UDINT;																							//Counter to check if the new value is stored in the buffer
	udiNumberLoopFull_ReadAds								: UDINT;																							//Number of the max Loop (dependet from max symbols in plc)
	udiNumberLoopHalf_ReadAds								: UDINT;																							//Number of the max Loop /2 (dependet from max symbols in plc)
	udiStartValueSymbolsRead_ReadAds						: UDINT;																							//Start value to read the symbols
	udiStopValueSymbolsRead_ReadAds							: UDINT;																							//Stop value to read the symbols
	udiRestValueSymbolRead_ReadAds							: UDINT;																							//Remain from the symbols not yet read 
	udiSizeReq_ReadAds										: UDINT;																							//Size from read request
	udiNumberSizeValues_ReadAds								: UDINT;																							//Number of symbols
	udiStartValueReadOutReqBuffer_AdsRead					: UDINT;																							//Start value to read out from the big buffer																						
	udiStopValueReadOutReqBuffer_AdsRead					: UDINT;																							//Stop value to read out from the big buffer
	udiNumberInProgress_ReadAds								: UDINT;																							//Number in prgogress from the 10 read out values
	uliValueDateTime_ReadAds								: ULINT;																							//Value Date and time after read out in milliseconds
	sStringFloat_ReadAds									: STRING(510);																						//String to check the float value
	(*-----------------------------------------------------------------------------------------ADS WRITE-------------------------------------------------------------------------------------------------------*)
	fbADSREADWRITE_WriteAds									: ADSRDWRT;																							//ADS read/write function
	fbADSWrite_WriteAds										: ADSWRITE;																							//ADS write function
	FPErrorWrite_WriteAds									: R_TRIG;																							//Internal rising edge 
	FPErrorReadWrite_WriteAds								: R_TRIG;																							//Internal rising edge 
	timTimout_WriteAds										: TON;																								//Timout timer
	unConvertDWORD_TO_REAL_WriteAds							: ST_UN_FLOAT_CONVERT_DWORD_REAL;																	//Union to convert numbers
	unConvertULINT_TO_LREAL_WriteAds						: ST_UN_FLOAT_CONVERT_ULINT_LREAL;																	//Union to convert numbers
	arrBufferWriteDefault_WriteAds							: ARRAY[1..2000] OF BYTE;																			//Default Value to write ofer ads
	arrBufferWrite_WriteAds									: ARRAY[1..2000] OF BYTE;																			//Value to write ofer ads
	arrCounterWriteValueInArray_WriteAds					: ARRAY[1..8] OF UINT;																				//Counter to write the received value in the array
	arrValueDefault											: ARRAY[1..Lynus_Standards.Constants_Communicator.bySizeOfValueArray] OF STRING(30);				//Default values
	bError_WriteAds											: BOOL;																								//Error flag
	bValueValid_WriteAds									: BOOL;																								//True if the received value for write over ads is valid
	byCounterWaitInStep_WriteAds							: BYTE;																								//Wait counter in statemschine
	byCounterEncodeString_WriteAds							: BYTE;																								//Counter to encode the received String befor write ofer ads
	byCounterReceivedValueOK_WriteAds						: BYTE;																								//Counter for fill write array
	byLPEncodeReceivedString_WriteAds						: BYTE;																								//Loop to encode the received string
	iState_WriteAds											: INT := -2;																						//State maschine to write ADS
	uiInternError_WriteAds									: UINT;																								//Intern Error Code Write ads
	uiSizeToWrite_WriteAds									: UINT;																								//Size from the value befor write oder ads
	uiLPWriteValueFromArray_WriteAds						: UINT;																								//Loop to write the values from received messege in the write buffer
	uiCounterEncodeString_WriteAds							: UINT;																								//Counter to encode the received String befor write ofer ads
	uiWaitBySymbolString_WriteAds							: UINT;																								//If array is located, extend wait step due to ads write access
	udiDate_WriteAds										: UDINT;																							//Received Date or Daten and time
	udiLPWriteInReadArrayForCheck_WriteADS					: UDINT;																							//Loop to write the new Value in the read array. If the value not can write with success, the old read value is pushing in next step to the cloud 
	dwVarHndl_WriteAds										: DWORD;																							//Handle to read or write on the variable
	uliDate_WriteAds										: ULINT;																							//Received Date or Daten and time
	rRealValueCheckBeforWrite_WriteAds						: REAL;																								//Need to check the Real Value befror write in Buffer
	lrRealValueCheckBeforWrite_WriteAds						: LREAL;																							//Need to check the lReal Value befror write in Buffer
END_VAR
]]></Declaration>
    <Implementation>
      <ST><![CDATA[//Creator : Kai Ebensperger
//Company : Lynus AG
//Date : 07.04.2020
//Version : 1.0.0.0

//With this function its possible to read and write data from the symbols that were first found with the info Var find function
//The read function from the values of the symbols is cyclically, because directly on the plc is not possible to work with notifications 
//The read function always reads 10 variables at the same time. (Block Mode)
//At the beginning all variables are saved.
//After that, the whole direction works based on the buffer event, i.e. the value is only written to the buffer if it also changes
//If the send interface is free, the messages are sent from the buffer. Typ FIFO
//The function only starts when the scan module has been successfully completed and a connection to the cloud has been established.
//If there are several read and write functions on the PLC, then divide up the number of symbols. (Max 10 read write functions on 1 plc)

//Define Array Number for this function
IF Lynus_Standards.GVL_Communicator.bBusyInfoAlVariables OR Lynus_Standards.GVL_Communicator.bErrorInfoAlVariables OR NOT Lynus_Standards.GVL_Communicator.bInfoAlVariablesOK THEN 
	byReadWriteArrayNumber := 0; 
		iState_ReadAds := -10;
			iState_WriteAds := -2;
				timTimerStart.IN := FALSE; 	
END_IF  

IF byReadWriteArrayNumber = 0 AND NOT Lynus_Standards.GVL_Communicator.bBusyInfoAlVariables AND Lynus_Standards.GVL_Communicator.bInfoAlVariablesOK THEN
	FOR byLPReadWriteArrayNumber := 1 TO 10 BY 1 DO
		IF NOT Lynus_Standards.GVL_Communicator.arrReadWriteArrayNumberInUse[byLPReadWriteArrayNumber] THEN
			byReadWriteArrayNumber := Lynus_Standards.GVL_Communicator.arrReadWriteArrayNumber[byLPReadWriteArrayNumber];
				Lynus_Standards.GVL_Communicator.arrReadWriteArrayNumberInUse[byLPReadWriteArrayNumber] := TRUE;
					timTimerStart.IN := TRUE; 
						EXIT;
		END_IF	 	 
	END_FOR
END_IF

timTimerStart(IN:= , PT:= T#500MS * byReadWriteArrayNumber, Q=> , ET=> );

//Start from here with the general code. All of the code is inside this IF instruction 
IF byReadWriteArrayNumber <> 0 AND timTimerStart.Q THEN

byReadWriteArrayNumber := LIMIT(1,byReadWriteArrayNumber,10);

//TC3 Port
uiPort := TwinCAT_SystemInfoVarList._AppInfo.AdsPort;

//AMS Net ID
fbNedID();
	IF (NOT fbNedID.bBusy) AND NOT fbNedID.bError THEN fbNedID.bExecute := FALSE; END_IF

//TaskIndex
fbTaskIndex(index=> );
	udiCycleTime := TwinCAT_SystemInfoVarList._TaskInfo[fbTaskIndex.index].CycleTime / 10000;	
	
//Wait in Step min 3 x Cycle Time for ADS Response
byStaticValueWaitInStep := STRING_TO_BYTE(LREAL_TO_FMTSTR((UDINT_TO_LREAL(udiCycleTime) / 10) * 3,0,FALSE)); 
	byStaticValueWaitInStep := LIMIT(3,byStaticValueWaitInStep,30);	
 
//Timeout
IF iState_ReadAds <> -8 AND iState_ReadAds <> -9 AND iState_ReadAds <> -4 THEN timTimout_ReadAds.IN := TRUE; ELSE timTimout_ReadAds.IN := FALSE; END_IF
	timTimout_ReadAds(IN:= , PT:= T#30S, Q=> , ET=> );	

IF iState_WriteAds <> 0 AND iState_WriteAds <> -1 THEN timTimout_WriteAds.IN := TRUE; ELSE timTimout_WriteAds.IN := FALSE; END_IF
	timTimout_WriteAds(IN:= , PT:= T#30S, Q=> , ET=> );	
	
//Back to init if the search fb is busy
FPInitSendError(CLK:= Lynus_Standards.GVL_Communicator.arrInterfaceInfoValueSendTo[byReadWriteArrayNumber].bErrorSending, Q=> );
	IF FPInitSendError.Q THEN iState_ReadAds := -10; iState_WriteAds := -2; END_IF 

//Number for Loop of exists symbols
udiNumberLoopFull_ReadAds := Lynus_Standards.GVL_Communicator.arrCounterVariableInArray[byReadWriteArrayNumber];
	udiNumberLoopFull_ReadAds := LIMIT(1,udiNumberLoopFull_ReadAds,Lynus_Standards.Constants_Communicator.udiArraySizeMarkedSymobls);

udiNumberLoopHalf_ReadAds := STRING_TO_UDINT(LREAL_TO_FMTSTR((UDINT_TO_LREAL(udiNumberLoopFull_ReadAds) / 2),0,TRUE));
	udiNumberLoopHalf_ReadAds := LIMIT(1,udiNumberLoopHalf_ReadAds,STRING_TO_UDINT(LREAL_TO_FMTSTR((UDINT_TO_LREAL(udiNumberLoopFull_ReadAds) / 2),0,TRUE)));	
	
//Data write over ADS

CASE iState_WriteAds OF
	
	-2://Reset Write ADS (When deleting the array, conserve resources because the code must also work on small PLCs)
		Lynus_Standards.GVL_Communicator.arrInterfaceInfoValueWriteTo[byReadWriteArrayNumber].bWriteNewValue := FALSE; Lynus_Standards.GVL_Communicator.arrInterfaceInfoValueWriteTo[byReadWriteArrayNumber].bBusy := FALSE; Lynus_Standards.GVL_Communicator.arrInterfaceInfoValueWriteTo[byReadWriteArrayNumber].bErrorWrite := FALSE;
			bError_WriteAds := FALSE; bValueValid_WriteAds := FALSE;
				MEMCPY(ADR(arrBufferWrite_WriteAds),ADR(arrBufferWriteDefault_WriteAds),SIZEOF(arrBufferWrite_WriteAds));
					uiInternError_WriteAds := 0; byCounterEncodeString_WriteAds := 0; udiDate_WriteAds := 0; uiSizeToWrite_WriteAds := 0; byCounterReceivedValueOK_WriteAds := 0; uiCounterEncodeString_WriteAds := 0; uliDate_WriteAds := 0;
						byCounterWaitInStep_WriteAds := 0; dwVarHndl_WriteAds := 0;

		iState_WriteAds := -1;

	-1://Wait from info symbol function (When deleting the array, conserve resources because the code must also work on small PLCs)
		IF NOT Lynus_Standards.GVL_Communicator.bBusyInfoAlVariables AND NOT Lynus_Standards.GVL_Communicator.bErrorInfoAlVariables AND Lynus_Standards.GVL_Communicator.bInfoAlVariablesOK AND NOT Lynus_Standards.GVL_Communicator.arrInterfaceInfoValueSendTo[byReadWriteArrayNumber].bErrorSending AND
			bFirstReadCycleDone_ReadAds AND Lynus_Standards.GVL_Communicator.arrInterfaceInfoValueWriteTo[byReadWriteArrayNumber].bSendFunctionOK THEN 
				iState_WriteAds := 0;
		END_IF

	0://Init and clear write array befor wirte the new value (When deleting the array, conserve resources because the code must also work on small PLCs)
		Lynus_Standards.GVL_Communicator.arrInterfaceInfoValueWriteTo[byReadWriteArrayNumber].bErrorWrite := FALSE;
			arrCounterWriteValueInArray_WriteAds[1] := 0; arrCounterWriteValueInArray_WriteAds[2] := 0; arrCounterWriteValueInArray_WriteAds[3] := 0; arrCounterWriteValueInArray_WriteAds[4] := 0; arrCounterWriteValueInArray_WriteAds[5] := 0;
				arrCounterWriteValueInArray_WriteAds[6] := 0; arrCounterWriteValueInArray_WriteAds[7] := 0; arrCounterWriteValueInArray_WriteAds[8] := 0;
					rRealValueCheckBeforWrite_WriteAds := 0; lrRealValueCheckBeforWrite_WriteAds := 0; 
	
		IF Lynus_Standards.GVL_Communicator.arrInterfaceInfoValueWriteTo[byReadWriteArrayNumber].bWriteNewValue AND Lynus_Standards.GVL_Communicator.arrInterfaceInfoValueWriteTo[byReadWriteArrayNumber].sFullSymbolName <> '' THEN 
			iState_WriteAds := 1; 
				Lynus_Standards.GVL_Communicator.arrInterfaceInfoValueWriteTo[byReadWriteArrayNumber].bBusy := TRUE;
					MEMCPY(ADR(arrBufferWrite_WriteAds),ADR(arrBufferWriteDefault_WriteAds),SIZEOF(arrBufferWrite_WriteAds));
		END_IF

	1://Open var handle (When deleting the array, conserve resources because the code must also work on small PLCs)
		fbADSREADWRITE_WriteAds.WRITELEN := SIZEOF(Lynus_Standards.GVL_Communicator.arrInterfaceInfoValueWriteTo[byReadWriteArrayNumber].sFullSymbolName); 	
			fbADSREADWRITE_WriteAds.SRCADDR := ADR(Lynus_Standards.GVL_Communicator.arrInterfaceInfoValueWriteTo[byReadWriteArrayNumber].sFullSymbolName);
				fbADSREADWRITE_WriteAds.WRTRD := TRUE;	
		
		IF NOT fbADSREADWRITE_WriteAds.BUSY THEN byCounterWaitInStep_WriteAds := byCounterWaitInStep_WriteAds + 1; END_IF  		
	
		IF NOT fbADSREADWRITE_WriteAds.BUSY AND NOT fbADSREADWRITE_WriteAds.ERR AND byCounterWaitInStep_WriteAds >= byStaticValueWaitInStep AND dwVarHndl_WriteAds <> 0 THEN byCounterWaitInStep_WriteAds := 0; fbADSREADWRITE_WriteAds.WRTRD := FALSE; iState_WriteAds := 2; END_IF 
		
			IF FPErrorReadWrite_WriteAds.Q OR timTimout_WriteAds.Q THEN iState_WriteAds := 200; END_IF
			
	2://Prepear Write array befor write over ads
		//Standart Value
		//Type BOOL
		IF Lynus_Standards.GVL_Communicator.arrInterfaceInfoValueWriteTo[byReadWriteArrayNumber].sSymbolType = 'BOOL' AND TO_LWORD(Lynus_Standards.GVL_Communicator.arrInterfaceInfoValueWriteTo[byReadWriteArrayNumber].arrValue[1]) >= 0 AND TO_LWORD(Lynus_Standards.GVL_Communicator.arrInterfaceInfoValueWriteTo[byReadWriteArrayNumber].arrValue[1]) <= 1 THEN 
			arrBufferWrite_WriteAds[1] := STRING_TO_BYTE(Lynus_Standards.GVL_Communicator.arrInterfaceInfoValueWriteTo[byReadWriteArrayNumber].arrValue[1]); 
				bValueValid_WriteAds := TRUE;
					uiSizeToWrite_WriteAds := 1;
		END_IF
		//Type BYTE  	
		IF Lynus_Standards.GVL_Communicator.arrInterfaceInfoValueWriteTo[byReadWriteArrayNumber].sSymbolType = 'BYTE' AND TO_LWORD(Lynus_Standards.GVL_Communicator.arrInterfaceInfoValueWriteTo[byReadWriteArrayNumber].arrValue[1]) >= 0 AND TO_LWORD(Lynus_Standards.GVL_Communicator.arrInterfaceInfoValueWriteTo[byReadWriteArrayNumber].arrValue[1]) <= 255 THEN 
			arrBufferWrite_WriteAds[1] := STRING_TO_BYTE(Lynus_Standards.GVL_Communicator.arrInterfaceInfoValueWriteTo[byReadWriteArrayNumber].arrValue[1]); 
				bValueValid_WriteAds := TRUE;
					uiSizeToWrite_WriteAds := 1;
		END_IF
		//Type WORD
		IF Lynus_Standards.GVL_Communicator.arrInterfaceInfoValueWriteTo[byReadWriteArrayNumber].sSymbolType = 'WORD' AND TO_LWORD(Lynus_Standards.GVL_Communicator.arrInterfaceInfoValueWriteTo[byReadWriteArrayNumber].arrValue[1]) >= 0 AND TO_LWORD(Lynus_Standards.GVL_Communicator.arrInterfaceInfoValueWriteTo[byReadWriteArrayNumber].arrValue[1]) <= 65535 THEN 
			arrBufferWrite_WriteAds[2] := WORD_TO_BYTE(SHR(STRING_TO_WORD(Lynus_Standards.GVL_Communicator.arrInterfaceInfoValueWriteTo[byReadWriteArrayNumber].arrValue[1]),8) AND 16#FFFF);
			arrBufferWrite_WriteAds[1] := WORD_TO_BYTE(STRING_TO_WORD(Lynus_Standards.GVL_Communicator.arrInterfaceInfoValueWriteTo[byReadWriteArrayNumber].arrValue[1]) AND 16#FFFF); 
				bValueValid_WriteAds := TRUE;
					uiSizeToWrite_WriteAds := 2;
		END_IF
