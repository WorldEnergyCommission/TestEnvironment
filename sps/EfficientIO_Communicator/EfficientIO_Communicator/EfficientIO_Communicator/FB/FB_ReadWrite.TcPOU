		ELSE
			IF udiActiveNumberOfSymbol_ReadAds < Lynus_Standards.GVL_Communicator.arrCounterVariableInArray[byReadWriteArrayNumber] AND udiNumberInProgress_ReadAds < udiNumberSizeValues_ReadAds THEN iState_ReadAds := -4; udiNumberInProgress_ReadAds := udiNumberInProgress_ReadAds + 1; END_IF
			IF (udiNumberInProgress_ReadAds >= udiNumberSizeValues_ReadAds AND iState_ReadAds = 5) THEN iState_ReadAds := -8; udiNumberInProgress_ReadAds := 0; END_IF
			IF udiActiveNumberOfSymbol_ReadAds >= Lynus_Standards.GVL_Communicator.arrCounterVariableInArray[byReadWriteArrayNumber] THEN iState_ReadAds := -8; udiActiveNumberOfSymbol_ReadAds := 0; udiNumberInProgress_ReadAds := 0; udiRestValueSymbolRead_ReadAds := 0; END_IF 
