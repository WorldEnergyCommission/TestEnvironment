											arrBufferWrite_WriteAds[uiCounterEncodeString_WriteAds] := F_ToASC('');
						END_FOR	 
					END_IF
						byCounterEncodeString_WriteAds := 0;
							uiCounterEncodeString_WriteAds := uiCounterEncodeString_WriteAds + 1;
								uiSizeToWrite_WriteAds := 1 * uiCounterEncodeString_WriteAds;
									byCounterReceivedValueOK_WriteAds := byCounterReceivedValueOK_WriteAds + 1;
										byCounterReceivedValueOK_WriteAds := LIMIT(0,byCounterReceivedValueOK_WriteAds,Lynus_Standards.Constants_Communicator.bySizeOfValueArray);
											IF byCounterReceivedValueOK_WriteAds >= Lynus_Standards.GVL_Communicator.arrInterfaceInfoValueWriteTo[byReadWriteArrayNumber].uiSizeArray THEN bValueValid_WriteAds := TRUE; END_IF 	
				END_FOR
			END_IF
			//Type TIME and TIME_OF_DAY
			IF (Lynus_Standards.GVL_Communicator.arrInterfaceInfoValueWriteTo[byReadWriteArrayNumber].uiSizeArray <= Lynus_Standards.Constants_Communicator.bySizeOfValueArray AND (FIND(Lynus_Standards.GVL_Communicator.arrInterfaceInfoValueWriteTo[byReadWriteArrayNumber].sSymbolType,'OF TIME') <> 0 OR FIND(Lynus_Standards.GVL_Communicator.arrInterfaceInfoValueWriteTo[byReadWriteArrayNumber].sSymbolType,'OF TIME_OF_DAY') <> 0)) THEN
				FOR uiLPWriteValueFromArray_WriteAds := 1 TO Lynus_Standards.GVL_Communicator.arrInterfaceInfoValueWriteTo[byReadWriteArrayNumber].uiSizeArray BY 1 DO 
					IF TO_LWORD(Lynus_Standards.GVL_Communicator.arrInterfaceInfoValueWriteTo[byReadWriteArrayNumber].arrValue[uiLPWriteValueFromArray_WriteAds]) >= 0 AND TO_LWORD(Lynus_Standards.GVL_Communicator.arrInterfaceInfoValueWriteTo[byReadWriteArrayNumber].arrValue[uiLPWriteValueFromArray_WriteAds]) <= 4294967295 THEN   
						arrCounterWriteValueInArray_WriteAds[1] := LIMIT(1,arrCounterWriteValueInArray_WriteAds[1],2000); arrCounterWriteValueInArray_WriteAds[2] := LIMIT(2,arrCounterWriteValueInArray_WriteAds[2],2000);
							arrCounterWriteValueInArray_WriteAds[3] := LIMIT(3,arrCounterWriteValueInArray_WriteAds[3],2000); arrCounterWriteValueInArray_WriteAds[4] := LIMIT(4,arrCounterWriteValueInArray_WriteAds[4],2000);	
								arrBufferWrite_WriteAds[arrCounterWriteValueInArray_WriteAds[4]] := UDINT_TO_BYTE(SHR(STRING_TO_UDINT(Lynus_Standards.GVL_Communicator.arrInterfaceInfoValueWriteTo[byReadWriteArrayNumber].arrValue[uiLPWriteValueFromArray_WriteAds]),24) AND 16#FFFFFFFF);
								arrBufferWrite_WriteAds[arrCounterWriteValueInArray_WriteAds[3]] := UDINT_TO_BYTE(SHR(STRING_TO_UDINT(Lynus_Standards.GVL_Communicator.arrInterfaceInfoValueWriteTo[byReadWriteArrayNumber].arrValue[uiLPWriteValueFromArray_WriteAds]),16) AND 16#FFFFFFFF);
								arrBufferWrite_WriteAds[arrCounterWriteValueInArray_WriteAds[2]] := UDINT_TO_BYTE(SHR(STRING_TO_UDINT(Lynus_Standards.GVL_Communicator.arrInterfaceInfoValueWriteTo[byReadWriteArrayNumber].arrValue[uiLPWriteValueFromArray_WriteAds]),8) AND 16#FFFFFFFF);
								arrBufferWrite_WriteAds[arrCounterWriteValueInArray_WriteAds[1]] := UDINT_TO_BYTE(STRING_TO_UDINT(Lynus_Standards.GVL_Communicator.arrInterfaceInfoValueWriteTo[byReadWriteArrayNumber].arrValue[uiLPWriteValueFromArray_WriteAds]) AND 16#FFFFFFFF);  
						arrCounterWriteValueInArray_WriteAds[1] := arrCounterWriteValueInArray_WriteAds[1] + 4; arrCounterWriteValueInArray_WriteAds[2] := arrCounterWriteValueInArray_WriteAds[2] + 4;
							arrCounterWriteValueInArray_WriteAds[3] := arrCounterWriteValueInArray_WriteAds[3] + 4; arrCounterWriteValueInArray_WriteAds[4] := arrCounterWriteValueInArray_WriteAds[4] + 4;
								uiSizeToWrite_WriteAds := 4 * Lynus_Standards.GVL_Communicator.arrInterfaceInfoValueWriteTo[byReadWriteArrayNumber].uiSizeArray;	
									byCounterReceivedValueOK_WriteAds := byCounterReceivedValueOK_WriteAds + 1;
										byCounterReceivedValueOK_WriteAds := LIMIT(0,byCounterReceivedValueOK_WriteAds,Lynus_Standards.Constants_Communicator.bySizeOfValueArray);
											IF byCounterReceivedValueOK_WriteAds >= Lynus_Standards.GVL_Communicator.arrInterfaceInfoValueWriteTo[byReadWriteArrayNumber].uiSizeArray THEN bValueValid_WriteAds := TRUE; END_IF  
					END_IF
				END_FOR
			END_IF
			//Type DATE and DATE_AND_TIME
			IF (Lynus_Standards.GVL_Communicator.arrInterfaceInfoValueWriteTo[byReadWriteArrayNumber].uiSizeArray <= Lynus_Standards.Constants_Communicator.bySizeOfValueArray AND (FIND(Lynus_Standards.GVL_Communicator.arrInterfaceInfoValueWriteTo[byReadWriteArrayNumber].sSymbolType,'OF DATE') <> 0 OR FIND(Lynus_Standards.GVL_Communicator.arrInterfaceInfoValueWriteTo[byReadWriteArrayNumber].sSymbolType,'OF DATE_AND_TIME') <> 0)) THEN
				FOR uiLPWriteValueFromArray_WriteAds := 1 TO Lynus_Standards.GVL_Communicator.arrInterfaceInfoValueWriteTo[byReadWriteArrayNumber].uiSizeArray BY 1 DO 
					IF TO_LWORD(Lynus_Standards.GVL_Communicator.arrInterfaceInfoValueWriteTo[byReadWriteArrayNumber].arrValue[uiLPWriteValueFromArray_WriteAds]) >= 0 AND TO_LWORD(Lynus_Standards.GVL_Communicator.arrInterfaceInfoValueWriteTo[byReadWriteArrayNumber].arrValue[uiLPWriteValueFromArray_WriteAds]) <= 4294967295000 THEN 
						uliDate_WriteAds := STRING_TO_ULINT(Lynus_Standards.GVL_Communicator.arrInterfaceInfoValueWriteTo[byReadWriteArrayNumber].arrValue[uiLPWriteValueFromArray_WriteAds]);  
							udiDate_WriteAds := ULINT_TO_UDINT(uliDate_WriteAds / 1000);
								arrCounterWriteValueInArray_WriteAds[1] := LIMIT(1,arrCounterWriteValueInArray_WriteAds[1],2000); arrCounterWriteValueInArray_WriteAds[2] := LIMIT(2,arrCounterWriteValueInArray_WriteAds[2],2000);
									arrCounterWriteValueInArray_WriteAds[3] := LIMIT(3,arrCounterWriteValueInArray_WriteAds[3],2000); arrCounterWriteValueInArray_WriteAds[4] := LIMIT(4,arrCounterWriteValueInArray_WriteAds[4],2000);	
										arrBufferWrite_WriteAds[arrCounterWriteValueInArray_WriteAds[4]] := UDINT_TO_BYTE(SHR(udiDate_WriteAds,24) AND 16#FFFFFFFF);
										arrBufferWrite_WriteAds[arrCounterWriteValueInArray_WriteAds[3]] := UDINT_TO_BYTE(SHR(udiDate_WriteAds,16) AND 16#FFFFFFFF);
										arrBufferWrite_WriteAds[arrCounterWriteValueInArray_WriteAds[2]] := UDINT_TO_BYTE(SHR(udiDate_WriteAds,8) AND 16#FFFFFFFF);
										arrBufferWrite_WriteAds[arrCounterWriteValueInArray_WriteAds[1]] := UDINT_TO_BYTE(udiDate_WriteAds AND 16#FFFFFFFF);  
								arrCounterWriteValueInArray_WriteAds[1] := arrCounterWriteValueInArray_WriteAds[1] + 4; arrCounterWriteValueInArray_WriteAds[2] := arrCounterWriteValueInArray_WriteAds[2] + 4;
									arrCounterWriteValueInArray_WriteAds[3] := arrCounterWriteValueInArray_WriteAds[3] + 4; arrCounterWriteValueInArray_WriteAds[4] := arrCounterWriteValueInArray_WriteAds[4] + 4;
										uiSizeToWrite_WriteAds := 4 * Lynus_Standards.GVL_Communicator.arrInterfaceInfoValueWriteTo[byReadWriteArrayNumber].uiSizeArray;	
											byCounterReceivedValueOK_WriteAds := byCounterReceivedValueOK_WriteAds + 1;
												byCounterReceivedValueOK_WriteAds := LIMIT(0,byCounterReceivedValueOK_WriteAds,Lynus_Standards.Constants_Communicator.bySizeOfValueArray);
													IF byCounterReceivedValueOK_WriteAds >= Lynus_Standards.GVL_Communicator.arrInterfaceInfoValueWriteTo[byReadWriteArrayNumber].uiSizeArray THEN bValueValid_WriteAds := TRUE; END_IF  
					END_IF
				END_FOR
			END_IF
			
			IF timTimout_WriteAds.Q OR NOT bValueValid_WriteAds THEN 
				iState_WriteAds := 200; 
					IF NOT bValueValid_WriteAds THEN uiInternError_WriteAds := 1; END_IF 
			ELSE
				iState_WriteAds := 3;
					bValueValid_WriteAds := FALSE;
						byCounterEncodeString_WriteAds := 0; udiDate_WriteAds := 0; byCounterReceivedValueOK_WriteAds := 0; uiCounterEncodeString_WriteAds := 0; uliDate_WriteAds := 0;
							arrCounterWriteValueInArray_WriteAds[1] := 0; arrCounterWriteValueInArray_WriteAds[2] := 0; arrCounterWriteValueInArray_WriteAds[3] := 0; arrCounterWriteValueInArray_WriteAds[4] := 0;
								arrCounterWriteValueInArray_WriteAds[5] := 0; arrCounterWriteValueInArray_WriteAds[6] := 0; arrCounterWriteValueInArray_WriteAds[7] := 0; arrCounterWriteValueInArray_WriteAds[8] := 0;	
			END_IF
		
	3://Write Value
		IF Lynus_Standards.GVL_Communicator.arrCounterVariableInArray[byReadWriteArrayNumber] >= 1 AND Lynus_Standards.GVL_Communicator.arrCounterVariableInArray[byReadWriteArrayNumber] <= Lynus_Standards.Constants_Communicator.udiArraySizeMarkedSymobls THEN 
			FOR udiLPWriteInReadArrayForCheck_WriteADS := 1 TO Lynus_Standards.GVL_Communicator.arrCounterVariableInArray[byReadWriteArrayNumber] BY 1 DO 
				IF Lynus_Standards.GVL_Communicator.arrInterfaceInfoValueWriteTo[byReadWriteArrayNumber].sFullSymbolName = stADSRead_ReadAds[udiLPWriteInReadArrayForCheck_WriteADS].sFullSymbolName THEN
					stADSRead_ReadAds[udiLPWriteInReadArrayForCheck_WriteADS].arrValue := Lynus_Standards.GVL_Communicator.arrInterfaceInfoValueWriteTo[byReadWriteArrayNumber].arrValue; 
						stADSReadNew_ReadAds[udiLPWriteInReadArrayForCheck_WriteADS].arrValue := Lynus_Standards.GVL_Communicator.arrInterfaceInfoValueWriteTo[byReadWriteArrayNumber].arrValue;	
							EXIT;		
				END_IF
			END_FOR	
		END_IF
		
		//Extensions of the waiting step until data has been successfully written via ads so that the old value is not read back too quickly
		uiWaitBySymbolString_WriteAds := REAL_TO_UINT(F_Scalvalue(x:= UDINT_TO_REAL(udiCycleTime), x1:= 1, y1:= 25, x2:= 10, y2:= 1));
			uiWaitBySymbolString_WriteAds := LIMIT(1,uiWaitBySymbolString_WriteAds,25);
		
		IF Lynus_Standards.GVL_Communicator.arrInterfaceInfoValueWriteTo[byReadWriteArrayNumber].uiSizeArray > 2 THEN 
			uiWaitBySymbolString_WriteAds := uiWaitBySymbolString_WriteAds + Lynus_Standards.GVL_Communicator.arrInterfaceInfoValueWriteTo[byReadWriteArrayNumber].uiSizeArray * 8;
		ELSE
			uiWaitBySymbolString_WriteAds := uiWaitBySymbolString_WriteAds + 20;
		END_IF
	
		IF FIND(Lynus_Standards.GVL_Communicator.arrInterfaceInfoValueWriteTo[byReadWriteArrayNumber].sSymbolType,'OF STRING(30)') <> 0 THEN uiWaitBySymbolString_WriteAds := uiWaitBySymbolString_WriteAds + (Lynus_Standards.GVL_Communicator.arrInterfaceInfoValueWriteTo[byReadWriteArrayNumber].uiSizeArray * 8); END_IF
		
			uiWaitBySymbolString_WriteAds := LIMIT(0,uiWaitBySymbolString_WriteAds,255);
		
		fbADSWrite_WriteAds.IDXGRP := ADSIGRP_SYM_VALBYHND;
			fbADSWrite_WriteAds.IDXOFFS := dwVarHndl_WriteAds;
				fbADSWrite_WriteAds.LEN := uiSizeToWrite_WriteAds;
					fbADSWrite_WriteAds.SRCADDR := ADR(arrBufferWrite_WriteAds);	
						IF byCounterWaitInStep_WriteAds >= 1 THEN fbADSWrite_WriteAds.WRITE := TRUE; END_IF	
	
			IF NOT fbADSWrite_WriteAds.BUSY THEN byCounterWaitInStep_WriteAds := byCounterWaitInStep_WriteAds + 1; END_IF
			
			IF NOT fbADSWrite_WriteAds.BUSY AND NOT fbADSWrite_WriteAds.ERR AND byCounterWaitInStep_WriteAds >= (byStaticValueWaitInStep + uiWaitBySymbolString_WriteAds) THEN 
				fbADSWrite_WriteAds.WRITE := FALSE; 
					iState_WriteAds := 4; 	
						byCounterWaitInStep_WriteAds := 0; 
							uiSizeToWrite_WriteAds := 0;  	
								Lynus_Standards.GVL_Communicator.arrInterfaceInfoValueWriteTo[byReadWriteArrayNumber].bWriteNewValue := FALSE;			
			END_IF

				IF FPErrorWrite_WriteAds.Q OR timTimout_WriteAds.Q THEN iState_WriteAds := 200; END_IF
						
	4://Close Handle
		fbADSWrite_WriteAds.IDXGRP := ADSIGRP_SYM_RELEASEHND;
			fbADSWrite_WriteAds.IDXOFFS := 0;
				fbADSWrite_WriteAds.LEN := SIZEOF(dwVarHndl_WriteAds);
					fbADSWrite_WriteAds.SRCADDR := ADR(dwVarHndl_WriteAds);	
						fbADSWrite_WriteAds.WRITE := TRUE;
							
			IF NOT fbADSWrite_WriteAds.BUSY THEN byCounterWaitInStep_WriteAds := byCounterWaitInStep_WriteAds + 1; END_IF
		
		IF NOT fbADSWrite_WriteAds.BUSY AND NOT fbADSWrite_WriteAds.ERR AND byCounterWaitInStep_WriteAds >= (byStaticValueWaitInStep) THEN 
			fbADSWrite_WriteAds.WRITE := FALSE; 
				Lynus_Standards.GVL_Communicator.arrInterfaceInfoValueWriteTo[byReadWriteArrayNumber].bBusy := FALSE;
					Lynus_Standards.GVL_Communicator.arrInterfaceInfoValueWriteTo[byReadWriteArrayNumber].bErrorWrite := FALSE;
						Lynus_Standards.GVL_Communicator.arrInterfaceInfoValueWriteTo[byReadWriteArrayNumber].sFullSymbolName := '';
							Lynus_Standards.GVL_Communicator.arrInterfaceInfoValueWriteTo[byReadWriteArrayNumber].sSymbolType := '';
								Lynus_Standards.GVL_Communicator.arrInterfaceInfoValueWriteTo[byReadWriteArrayNumber].arrValue := arrValueDefault;	
									Lynus_Standards.GVL_Communicator.arrInterfaceInfoValueWriteTo[byReadWriteArrayNumber].uiSizeArray := 0;
										bError_WriteAds := FALSE;
											uiInternError_WriteAds := 0;
												dwVarHndl_WriteAds := 0; 
													iState_WriteAds := 0; 	
														byCounterWaitInStep_WriteAds := 0;
		END_IF

			IF FPErrorWrite_WriteAds.Q OR timTimout_WriteAds.Q THEN iState_WriteAds := 200; END_IF	
		
	200://Error
		Lynus_Standards.GVL_Communicator.arrInterfaceInfoValueWriteTo[byReadWriteArrayNumber].bErrorWrite := TRUE; Lynus_Standards.GVL_Communicator.arrInterfaceInfoValueWriteTo[byReadWriteArrayNumber].bBusy := FALSE; Lynus_Standards.GVL_Communicator.arrInterfaceInfoValueWriteTo[byReadWriteArrayNumber].bWriteNewValue := FALSE;
			bError_WriteAds := TRUE; fbADSREADWRITE_WriteAds.WRTRD := FALSE; fbADSWrite_WriteAds.WRITE := FALSE; bValueValid_WriteAds := FALSE; Lynus_Standards.GVL_Communicator.arrInterfaceInfoValueWriteTo[byReadWriteArrayNumber].sFullSymbolName := '';
				dwVarHndl_WriteAds := 0; byCounterWaitInStep_WriteAds := 0; byCounterEncodeString_WriteAds := 0; udiDate_WriteAds := 0; uiSizeToWrite_WriteAds := 0; byCounterReceivedValueOK_WriteAds := 0; uiCounterEncodeString_WriteAds := 0;
					uliDate_WriteAds := 0;
		
		iState_WriteAds := 300;
	
	300://Wait in Error
		byCounterWaitInStep_WriteAds := byCounterWaitInStep_WriteAds + 1;
	
			IF byCounterWaitInStep_WriteAds > 10 THEN byCounterWaitInStep_WriteAds := 0; iState_WriteAds := 0; END_IF

END_CASE

