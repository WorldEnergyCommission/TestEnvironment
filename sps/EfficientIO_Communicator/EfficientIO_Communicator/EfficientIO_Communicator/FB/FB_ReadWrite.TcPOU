																											SHL(BYTE_TO_ULINT(arrValueAfterRead_ReadAds[arrCounterWriteValueInArray_ReadAds[4]]),24) + SHL(BYTE_TO_ULINT(arrValueAfterRead_ReadAds[arrCounterWriteValueInArray_ReadAds[3]]),16) + SHL(BYTE_TO_ULINT(arrValueAfterRead_ReadAds[arrCounterWriteValueInArray_ReadAds[2]]),8) + BYTE_TO_ULINT(arrValueAfterRead_ReadAds[arrCounterWriteValueInArray_ReadAds[1]])));
