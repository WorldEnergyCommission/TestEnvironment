					//ARRAY OF TIME or ARRAY OF TIME OF DAY	
					IF FIND(Lynus_Standards.GVL_Communicator.arrInterfaceInfoAlVariables[byReadWriteArrayNumber,udiActiveNumberOfSymbol_ReadAds].sSymbolType,'OF TIME') <> 0 OR FIND(Lynus_Standards.GVL_Communicator.arrInterfaceInfoAlVariables[byReadWriteArrayNumber,udiActiveNumberOfSymbol_ReadAds].sSymbolType,'OF TIME_OF_DAY') <> 0 THEN
						stADSRead_ReadAds[udiActiveNumberOfSymbol_ReadAds].sFullSymbolName := Lynus_Standards.GVL_Communicator.arrInterfaceInfoAlVariables[byReadWriteArrayNumber,udiActiveNumberOfSymbol_ReadAds].sFullSymbolName;
							stADSRead_ReadAds[udiActiveNumberOfSymbol_ReadAds].arrValue := arrValueDefault_ReadAds;	
								IF Lynus_Standards.GVL_Communicator.arrInterfaceInfoAlVariables[byReadWriteArrayNumber,udiActiveNumberOfSymbol_ReadAds].uiSizeArray <= Lynus_Standards.Constants_Communicator.bySizeOfValueArray THEN  
									FOR uiLPWriteValueByArray_ReadAds := 1 TO Lynus_Standards.GVL_Communicator.arrInterfaceInfoAlVariables[byReadWriteArrayNumber,udiActiveNumberOfSymbol_ReadAds].uiSizeArray BY 1 DO
										arrCounterWriteValueInArray_ReadAds[1] := LIMIT(1,arrCounterWriteValueInArray_ReadAds[1],1600); arrCounterWriteValueInArray_ReadAds[2] := LIMIT(2,arrCounterWriteValueInArray_ReadAds[2],1600);
											arrCounterWriteValueInArray_ReadAds[3] := LIMIT(3,arrCounterWriteValueInArray_ReadAds[3],1600); arrCounterWriteValueInArray_ReadAds[4] := LIMIT(4,arrCounterWriteValueInArray_ReadAds[4],1600);
												stADSRead_ReadAds[udiActiveNumberOfSymbol_ReadAds].arrValue[uiLPWriteValueByArray_ReadAds] := UDINT_TO_STRING(SHL(BYTE_TO_UDINT(arrValueAfterRead_ReadAds[arrCounterWriteValueInArray_ReadAds[4]]),24) + SHL(BYTE_TO_UDINT(arrValueAfterRead_ReadAds[arrCounterWriteValueInArray_ReadAds[3]]),16) + SHL(BYTE_TO_UDINT(arrValueAfterRead_ReadAds[arrCounterWriteValueInArray_ReadAds[2]]),8) + BYTE_TO_UDINT(arrValueAfterRead_ReadAds[arrCounterWriteValueInArray_ReadAds[1]]));
										arrCounterWriteValueInArray_ReadAds[1] := arrCounterWriteValueInArray_ReadAds[1] + 4; arrCounterWriteValueInArray_ReadAds[2] := arrCounterWriteValueInArray_ReadAds[2] + 4;
											arrCounterWriteValueInArray_ReadAds[3] := arrCounterWriteValueInArray_ReadAds[3] + 4; arrCounterWriteValueInArray_ReadAds[4] := arrCounterWriteValueInArray_ReadAds[4] + 4;
									END_FOR 
